  object Query2: TQuery
    DatabaseName = 'DB'
    DataSource = DS1
    SQL.Strings = (
      'Select CFMDate1'
      'From SGDH'
      'Where SGNO=:SGNO')
    Left = 85
    Top = 406
    ParamData = <
      item
        DataType = ftFixedChar
        Name = 'SGNO'
        ParamType = ptUnknown
        Size = 12
      end>
