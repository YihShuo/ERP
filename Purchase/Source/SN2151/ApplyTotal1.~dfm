  object SGDet: TQuery
    AfterOpen = SGDetAfterOpen
    OnCalcFields = SGDetCalcFields
    DatabaseName = 'DB'
    DataSource = DS1
    SQL.Strings = (
      
        'select SGDHS.SGNO,SGDHS.CLBH,SGDHS.SGQty,SGDHS.Qty,SGDHS.USPrice' +
        ',SGDHS.VNPrice,SGDHS.YQdate,SGDHS.Memo,SGDHS.BJNO'
      '       ,SGDHS.USERDate,SGDHS.USERID,SGDHS.YN'
      
        '       ,CLZL.YWPM,CLZL.ZWPM,ZSZL.ZSDH,ZSZL.zsywjc,CLZL.dwbh,LBZL' +
        'S.ZWSM'
      
        '     '#9',case when SGDHS.USPrice is null then SGDHS.VNPrice*Qty el' +
        'se CWHLS.CWHL*SGDHS.USPrice*Qty end as ACC'
      
        #9'    ,IsNull((Select Sum(Qty) from KCCLDAY_BDepartment where Dep' +
        'ID=SGDH.DepID and SGDHS.CLBH=CLBH),0.0) as DepQty'
      
        #9'    ,IsNull((Select Sum(Qty) from KCCLDAY_BDepartment where Dep' +
        'ID='#39'ZZZZZZZZZZ'#39' and SGDHS.CLBH=CLBH),0.0) as ZZZQty'
      #9#9',SGDHS.YM,SGDHS.Season,SGDHS.Serno'
      'from SGDHS'
      'left join SGDH on SGDH.SGNO=SGDHS.SGNO'
      'left join CLZL on CLZL.CLDH = SGDHS.CLBH'
      'Left join CGBJ on CGBJ.BJNO=SGDHS.BJNO'
      'left join ZSZL on ZSZL.ZSDH = CGBJ.ZSBH'
      'Left join LBZLS on LBZLS.LBDH=CLZL.dwbh and LBZLS.LB='#39'02'#39
      
        'left join CWHLS on CWHLS.HLDAY='#39'01'#39' and CWHLS.HLMONTH=MONTH(GETD' +
        'ATE()) and CWHLS.HLYEAR=YEAR(GETDATE())'
      'where SGDHS.SGNO=:SGNO'
      'order by SGDHS.SGNO'
      ''
      '')
