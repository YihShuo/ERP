  object UpMas: TUpdateSQL
    ModifySQL.Strings = (
      'update SGDH'
      'Set'
      '  DepID =:DepID,'
      '  LB =:LB,'
      '  Memo =:Memo,'
      '  SGDate =:SGDate,'
      '  CHKID =:CHKID,'
      '  CHKDATE =:CHKDATE,'
      '  USERID =:USERID,'
      '  USERDATE =:USERDATE,'
      '  CFMMemo =:CFMMemo,'
      '  CFMID =:CFMID,'
      '  CFMDATE =:CFMDATE,'
      '  flowflag=:flowflag,'
      '  Budget_chk=:Budget_chk,'
      '  Customer_Account=:Customer_Account,'
      '  Uniform = :Uniform'
      'where'
      '  SGNO =:OLD_SGNO')
    InsertSQL.Strings = (
      'insert into SGDH'
      
        '  (SGNO, GSBH, LB, DepID, Memo, SGDate ,USERID, USERDATE, CHKID,' +
        ' CHKDATE, CFMID, CFMDATE , CFMID1, CFMDATE1, YN, flowflag, Budge' +
        't_chk,Customer_Account, Uniform, OrderDate)'
      'values'
      
        '  (:SGNO, :GSBH, :LB, :DepID, :Memo, :SGDate, :USERID, :USERDATE' +
        ', :CHKID, :CHKDATE, :CFMID, :CFMDATE, :CFMID1, :CFMDATE1, :YN, :' +
        'flowflag, :Budget_chk, :Customer_Account, :Uniform, :OrderDate)')
    DeleteSQL.Strings = (
      'delete from SGDH'
      'where'
      '  SGNO = :OLD_SGNO')
    Left = 292
    Top = 469
  end
