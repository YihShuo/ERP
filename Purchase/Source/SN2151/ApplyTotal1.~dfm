  object UPDet: TUpdateSQL
    ModifySQL.Strings = (
      'update SGDHS'
      'set'
      '  CLBH=:CLBH,'
      '  SGQty =:SGQty,'
      '  Qty =:Qty,'
      '  VNPrice =:VNPrice,'
      '  USPrice =:USPrice,'
      '  YQdate =:YQdate,'
      '  BJNO =:BJNO,'
      '  Memo =:Memo,'
      '  USERID =:USERID,'
      '  USERDATE =:USERDATE,'
      '  YN =:YN,'
      '  YM =:YM,'
      '  Season =:Season,'
      '  Serno =:Serno'
      'where'
      '  SGNO =:OLD_SGNO and '
      '  CLBH=:OLD_CLBH'
      '')
    InsertSQL.Strings = (
      'insert into SGDHS'
      
        '  (SGNO, CLBH, SGQty, Qty, VNPrice, USPrice, YQdate, Memo, BJNO,' +
        ' USERDATE, USERID, YN, YM, Season, Serno)'
      'values'
      
        '  (:SGNO, :CLBH, :SGQty, :Qty, :VNPrice, :USPrice, :YQdate, :Mem' +
        'o, :BJNO, :USERDATE, :USERID, :YN, :YM, :Season, :Serno)'
      '')
    DeleteSQL.Strings = (
      'delete from SGDHS'
      'where'
      '  SGNO = :OLD_SGNO'
      '  and CLBH =:OLD_CLBH'
      ''
      ''
      '')
