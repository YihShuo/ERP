  object UpOrdMat: TUpdateSQL
    ModifySQL.Strings = (
      'update CGZLSS'
      'set'
      '  CGNO = :CGNO,'
      '  CLBH = :CLBH,'
      '  ZLBH = :ZLBH,'
      '  XXCC = :XXCC,'
      '  Qty = :Qty,'
      '  CLSL = :CLSL,'
      '  YQDate = :YQDate,'
      '  CFMDate = :CFMDate,'
      '  USERDate = :USERDate,'
      '  USERID = :USERID,'
      '  YN = :YN'
      'where'
      '  CGNO = :OLD_CGNO and'
      '  CLBH = :OLD_CLBH and'
      '  ZLBH = :OLD_ZLBH and'
      '  XXCC = :OLD_XXCC and'
      '  Stage = :OLD_Stage')
    InsertSQL.Strings = (
      'insert into CGZLSS'
      
        '  (CGNO, CLBH, ZLBH, XXCC, Stage, Qty, CLSL, YQDate, CFMDate, US' +
        'ERDate, USERID,'
      '   YN)'
      'values'
      
        '  (:CGNO, :CLBH, :ZLBH, :XXCC, :Stage, :Qty, :CLSL, :YQDate, :CF' +
        'MDate, :USERDate, '
      '   :USERID, :YN)')
    DeleteSQL.Strings = (
      'delete from CGZLSS'
      'where'
      '  CGNO = :OLD_CGNO and'
      '  CLBH = :OLD_CLBH and'
      '  ZLBH = :OLD_ZLBH and'
      '  XXCC = :OLD_XXCC')
    Left = 620
    Top = 404
  end
