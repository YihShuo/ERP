  object CGS: TQuery
    DatabaseName = 'DB'
    DataSource = DS4
    SQL.Strings = (
      
        'select CGZLSS.*,DDZL.Article,XXZl.XieMing,YWPM,IsNull(CGKCUSES.Q' +
        'ty,0) as UseStock'
      'from CGZLSS '
      'left join DDZL on CGZLSS.ZLBH=DDZL.DDBH '
      
        'left join XXZl on XXZl.XieXing=DDZl.XieXing and XXZl.SheHao=DDZl' +
        '.SheHao '
      'left join clzl on CGZLSS.CLBH=CLZL.CLDH'
      
        'left join (Select GSBH,ZLBH,CLBH,SIZE,Sum(Qty) as Qty from CGKCU' +
        'SES where CLBH=:CLBH and CGKCUSES.SIZE='#39'ZZZZZZ'#39' Group by  GSBH,Z' +
        'LBH,CLBH,SIZE )  CGKCUSES'
      
        '          on CGKCUSES.ZLBH=CGZLSS.ZLBH and CGKCUSES.CLBH=CGZLSS.' +
        'CLBH and CGKCUSES.SIZE=CGZLSS.XXCC and CGKCUSES.GSBH=:GSBH'
      'where CGZLSS.CGNO=:CGNO and CGZLSS.CLBH=:CLBH'
      'order by CGZLSS.ZLBH,CGZLSS.XXCC')
    UpdateObject = UpOrdMat
    Left = 620
    Top = 372
    ParamData = <
