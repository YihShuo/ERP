  object BuyNoQ: TQuery
    DatabaseName = 'DB'
    DataSource = DS3
    SQL.Strings = (
      
        'select DDZL.GSBH,MAX(DDZL.BUYNO) as BUYNO,Max(KFZL.KFJC) as KFJC' +
        ',Max(DDZL.RYType) as RYType from DDZL'
      'left join KFZL on KFZL.KFDH=DDZL.KHBH'
      
        'where DDZL.DDBH in (select ZLBH from CGZLSS where CGNO=:CGNO GRO' +
        'UP by ZLBH)'
      'and DDZL.GSBH=:GSBH'
      'group by DDZL.GSBH')
