  object CGDet: TQuery
    AfterOpen = CGDetAfterOpen
    OnCalcFields = CGDetCalcFields
    DatabaseName = 'DB'
    DataSource = DS3
    SQL.Strings = (
      
        'select CGZLS.*,CLZL.YWPM,CLZL.DWBH,CLZL.ZWPM,KCZK.ZKBH,KCZK.ZKQt' +
        'y,KCZK.ZKLQty  '
      'from CGZLS'
      'left join CLZL on CLZL.CLDH=CGZLS.CLBH'
      'Left join ('
      
        '      Select KCZK.CLBH,max(KCZK.Memo) as Memo,Max(KCZK.ZKBH) as ' +
        'ZKBH,Sum(KCZKS.Qty) as ZKQty,Sum(KCZKS.LQty) as ZKLQty'
      '      from KCZK'
      '      left join KCZKS on KCZKS.ZKBH=KCZK.ZKBH'
      
        '      where KCZK.YN<>'#39'X'#39' and KCZK.ZKNO='#39'ZZZZZZZZZZ'#39' and KCZK.GSB' +
        'H=:GSBH and KCZK.USERDate>GetDate()-180 and KCZKS.LQty>=0  and K' +
        'CZKS.SIZE='#39'ZZZZZZ'#39
      '      Group by KCZK.CLBH'
      '      having(Sum(KCZKS.LQty))>=0  ) KCZK on KCZK.CLBH=CGZLS.CLBH'
      'where CGZLS.CGNO=:CGNO'
      'order by CGZLS.CLBH ')
    UpdateObject = UPDet
    Left = 516
    Top = 372
    ParamData = <
      item
        DataType = ftFixedChar
        Name = 'GSBH'
        ParamType = ptUnknown
        Size = 5
      end
      item
        DataType = ftFixedChar
        Name = 'CGNO'
        ParamType = ptUnknown
        Size = 12
      end>
    object CGDetCGNO: TStringField
      FieldName = 'CGNO'
      FixedChar = True
      Size = 11
