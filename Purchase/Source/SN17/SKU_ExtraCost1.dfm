  object ExtraVN: TQuery
    AfterOpen = ExtraVNAfterOpen
    DatabaseName = 'DB'
    DataSource = DS1
    SQL.Strings = (
      
        'select  EXTRAVN.*,BWZL.YWSM,ZSZL.ZSYWJC,CLZL.YWPM,CLZL.ZWPM,CLZL' +
        '.DWBH'
      'from EXTRAVN'
      'left join BWZL on BWZL.BWDH= EXTRAVN.BWBH'
      'left join ZSZL on ZSZL.ZSDH= EXTRAVN.CSBH'
      'left join CLZL on EXTRAVN.CLBH=CLZL.CLDH'
      'where  EXTRAVN.XieXing=:XieXing and  EXTRAVN.SheHao=:SheHao'
      'Union all'
      
        'select :XieXing as XieXing,:SheHao as SheHao,null as BWBH,CLDH a' +
        's CLBH,null as CSBH,null as VNPrice,'
      
        '       null as USPrice, 1 as CLSL,null as USERID,null as USERDAT' +
        'E,'#39'1'#39' as YN,null as YWSM,null as ZSYWJC,CLZL.YWPM,CLZL.ZWPM,CLZL' +
        '.DWBH'
      'from ClZL'
      'where CLDH like '#39'VJ%'#39'  and IsNull(CLZL.TYJH,'#39'N'#39')='#39'N'#39
      
        '      and CLDH not in (select CLBH from EXTRAVN where EXTRAVN.Xi' +
        'eXing=:XieXing and  EXTRAVN.SheHao=:SheHao)'
      ' order by EXTRAVN.BWBH asc')
    UpdateObject = UpExtraVN
