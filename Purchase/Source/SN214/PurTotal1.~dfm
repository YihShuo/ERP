  object CGS: TQuery
    DatabaseName = 'DB'
    DataSource = DS4
    SQL.Strings = (
      
        'select CGZLSS.*,BDepartment.Depname,SGDH.USERID as User_SG,USERN' +
        'AME'
      'from CGZLSS left join SGDH on SGDH.SGNO=CGZLSS.ZLBH'
      
        'left join BDepartment on BDepartment.ID=IsNull(SGDH.DepID,CGZLSS' +
        '.Memo)'
      'left join Busers on SGDH.USERID=Busers.USERID'
      'where CGZLSS.CGNO=:CGNO and CGZLSS.CLBH=:CLBH'
      'order by CGZLSS.ZLBH,CGZLSS.XXCC')
    UpdateObject = UpOrdMat
    Left = 620
    Top = 372
    ParamData = <
      item
        DataType = ftFixedChar
        Name = 'CGNO'
        ParamType = ptUnknown
        Size = 12
      end
      item
        DataType = ftFixedChar
        Name = 'CLBH'
        ParamType = ptUnknown
        Size = 16
      end>
