  object UPDet: TUpdateSQL
    ModifySQL.Strings = (
      'update CGZLS'
      'set'
      '  CGNO = :CGNO,'
      '  CLBH = :CLBH,'
      '  XqQty = :XqQty,'
      '  Qty = :Qty,'
      '  USPrice = :USPrice,'
      '  VNPrice = :VNPrice,'
      '  Memo = :Memo,'
      '  BJNO = :BJNO,'
      '  YQDate = :YQDate,'
      '  CFMDate = :CFMDate,'
      '  USERDate = :USERDate,'
      '  USERID = :USERID,'
      '  YN = :YN'
      'where'
      '  CGNO = :OLD_CGNO and'
      '  CLBH = :OLD_CLBH')
    InsertSQL.Strings = (
      'insert into CGZLS'
      
        '  (CGNO, CLBH, XqQty, Qty, USPrice, VNPrice, Memo, BJNO, YQDate,' +
        ' CFMDate, '
      '   USERDate, USERID, YN)'
      'values'
      
        '  (:CGNO, :CLBH, :XqQty, :Qty, :USPrice, :VNPrice, :Memo, :BJNO,' +
        ' :YQDate, '
      '   :CFMDate, :USERDate, :USERID, :YN)')
    DeleteSQL.Strings = (
      'delete from CGZLS'
      'where'
      '  CGNO = :OLD_CGNO and'
      '  CLBH = :OLD_CLBH')
    Left = 516
    Top = 402
  end
