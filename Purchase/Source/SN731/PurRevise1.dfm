  object UP_Color: TUpdateSQL
    ModifySQL.Strings = (
      'UPDATE  CGZL_Revise'
      'SET'
      '  CGNO = :CGNO,'
      '  CLBH = :CLBH,'
      '  Before_Qty = :Before_Qty,'
      '  After_Qty = :After_Qty,'
      '  ManagerID = :ManagerID,'
      '  ManagerDate = :ManagerDate '
      'WHERE'
      '  CHANGENO = :OLD_CHANGENO')
    InsertSQL.Strings = (
      'INSERT INTO CGZL_Revise'
      
        '  (CHANGENO, CGNO, GSBH, CLBH, Before_Qty, After_Qty, USERDate, ' +
        'USERID, ManagerID, ManagerDate, YN )'
      'VALUES'
      
        '  (:CHANGENO, :CGNO, :GSBH, :CLBH, :Before_Qty, :After_Qty, :USE' +
        'RDate, :USERID, :ManagerID, :ManagerDate, :YN)')
    DeleteSQL.Strings = (
      'DELETE FROM CGZL_Revise'
      'WHERE '
      '  CHANGENO = :OLD_CHANGENO')
    Left = 16
