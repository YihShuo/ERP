  object Query1: TQuery
    DatabaseName = 'DB'
    SQL.Strings = (
      
        'select KCLLS.LLNo,KCLLS.CLBH,CLZL.YWPM,KCLLS.DFL,KCLLS.CLSL,KCLL' +
        'S.SCBH,KCLLS.TempQty,KCLLS.Qty,KCLLS.MEMO,KCLLS.BLSB,KCLLS.USERI' +
        'D,KCLLS.USERDATE'
      
        '       ,KCLLS.YYBH,SCBLYY.YWSM,KCLL.CKBH,KCLLS.CLBH as MJBH,'#39'Ass' +
        'embly'#39' as DType,KCLL.CFMDate,KCLLS.VNPrice_HG as VNPrice,KCLLS.V' +
        'NACC_HG as VNACC,KCLLS.HGLB,'
      
        '       case when KCLLS.HGLB in ('#39'NK'#39','#39'KD'#39') then IsNull(KCLLS.CNO' +
        ',CLHG.HGBH) else '#39#39' end as CNO,CLHG.UnitC,CLHG.RateC'
      'from KCLLS'
      'left join SCBLYY on SCBLYY.YYBH=KCLLS.YYBH '
      'left join KCLL on KCLL.LLNO=KCLLS.LLNO '
      'left join CLZL on CLZL.CLDH=KCLLS.CLBH '
      'left join CLHG on CLHG.CLBH=KCLLS.CLBH'
      'where KCLLS.SCBH='#39'Y1712-0250'#39' '
      
        'and convert(smalldatetime,convert(varchar,KCLL.CFMDate))<='#39'2017/' +
        '10/20'#39' '
      '   and KCLL.CFMID<>'#39'NO'#39
      '   ')
