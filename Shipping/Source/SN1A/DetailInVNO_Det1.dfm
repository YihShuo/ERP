  object Query2: TQuery
    DatabaseName = 'DB'
    DataSource = DS1
    SQL.Strings = (
      
        'Select KCRK.RKNO,KCRK.CKBH,KCRK.SFL,KCRKS.CLBH,KCRKS.RKSB,KCRKS.' +
        'CGBH,KCRKS.Qty,'
      '       Case when KCRK.HGLB='#39'NK'#39' then Qty else 0 end  as NK,'
      '       Case when KCRK.HGLB='#39'TC'#39' then Qty else 0 end  as TC,'
