    ModifySQL.Strings = (
      'update DDZL_ZLZLS2'
      'set'
      '        CLSL=:CLSL,'
      '        MJBH=:MJBH,'
      '        USERID=:USERID,'
      '        USERDATE=:USERDATE'
      'WHERE'
      '   ZLBH=:OLD_ZLBH AND'
      '   BWBH=:OLD_BWBH AND'
      '   CLBH=:OLD_CLBH AND'
      '   SIZE=:OLD_SIZE AND'
      '   YN=:OLD_YN'
      ''
      '')
    InsertSQL.Strings = (
      'Insert into DDZL_ZLZLS2'
      
        '   (ZLBH, XH ,BWBH, CSBH, MJBH, CLBH, ZMLB, SIZE, CLSL, USAGE, U' +
        'SERID, USERDATE, YN)'
      'Values'
      
        '   (:ZLBH, :XH ,:BWBH, :CSBH, :MJBH, :CLBH, :ZMLB, :SIZE, :CLSL,' +
        ' :USAGE, :USERID, :USERDATE, :YN)')
