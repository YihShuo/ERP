  object Query2: TQuery
    DatabaseName = 'DB'
    SQL.Strings = (
      'if object_id('#39'tempdb..#Material'#39') is not null '
      'begin   '
      'drop table #Material'
      'end'
      ''
      
        'select zlzls2.ZLBH,zlzls2.xh,zlzls2.BWBH,zlzls2.clbh,zlzls2.mjbh' +
        ',zlzls2.clsl,zlzls2.USAGE,zlzls2.BWMCY,'
      
        #9#9'zlzls2.CLMCY,zlzls2.dwbh,zlzls2.LOSS,zlzls2.CQDH,zlzls2.JGYWSM' +
        ',zlzls2.XieXing,zlzls2.SheHao,'
      #9#9'zlzls2.CCQQ,zlzls2.CCQZ'
      'into #Material'
      'from ('
      
        'SELECT ZLZLS2.ZLBH , ZLZLS2.XH,ZLZLS2.bwbh ,zlzls2.clbh AS clbh ' +
        ',max(zlzls2.mjbh) AS mjbh ,'
      
        '              sum(zlzls2.clsl) AS clsl,(sum(zlzls2.clsl)/ddzl.pa' +
        'irs) AS USAGE,max(BWZL.ywsm) AS BWMCY ,'
      
        '              max(CLZL.ywpm) AS CLMCY ,max(CLZL.dwbh) AS dwbh,Is' +
        'Null(XXZLS.LOSS,0) as LOSS  ,CLZL.CQDH,XXZLS.JGYWSM ,DDZL.XieXin' +
        'g ,DDZL.SheHao'
      '              ,XXZLS.CCQQ,XXZLS.CCQZ'
      'FROM DDZL_ZLZLS2 AS ZLZLS2'
      'LEFT JOIN BWZL AS BWZL ON ZLZLS2.BWBH = BWZL.bwdh'
      'LEFT JOIN CLZL AS CLZL ON ZLZLS2.CLBH = CLZL.cldh'
      'LEFT JOIN DDZL AS DDZL ON ZLZLS2.ZLBH = DDZL.ZLBH'
      'LEFT JOIN XXZLS AS XXZLS ON DDZL.XIEXING = XXZLS.XIEXING'
      
        '         AND DDZL.SHEHAO = XXZLS.SHEHAO AND ZLZLS2.BWBH = XXZLS.' +
        'BWBH --AND ZLZLS2.CLBH = XXZLS.CLBH'
      'where ZLZLS2.ZLBH=:ZLBH and ZLZLS2.MJBH='#39'ZZZZZZZZZZ'#39
      
        'GROUP BY ZLZLS2.XH,zlzls2.zlbh,zlzls2.bwbh,zlzls2.clbh,XXZLS.CLS' +
        'L,CLZL.CQDH,XXZLS.LOSS,XXZLS.JGYWSM ,ddzl.pairs ,DDZL.XieXing ,D' +
        'DZL.SheHao,XXZLS.CCQQ,XXZLS.CCQZ'
      'union all'
      
        'Select ZLZLS2.ZLBH,zlzls2.xh,ZLZLS2.bwbh+'#39'-'#39' as bwbh,zlzls2.clbh' +
        ',max(zlzls2.mjbh) AS mjbh ,'
      
        '              sum(zlzls2.clsl) AS clsl,(sum(zlzls2.clsl)/ddzl.pa' +
        'irs) AS USAGE,'#39'-----'#39' AS BWMCY ,'
      
        '              max(CLZL.ywpm) AS CLMCY ,max(CLZL.dwbh) AS dwbh,XX' +
        'ZLS.LOSS  ,CLZL.CQDH,XXZLS.JGYWSM,DDZL.XieXing ,DDZL.SheHao'
      '              ,XXZLS.CCQQ,XXZLS.CCQZ'
      'from DDZL_zlzls2 AS ZLZLS2 '
      'LEFT JOIN BWZL AS BWZL ON ZLZLS2.BWBH = BWZL.bwdh '
      'LEFT JOIN CLZL AS CLZL ON ZLZLS2.CLBH = CLZL.cldh '
      'LEFT JOIN DDZL AS DDZL ON ZLZLS2.ZLBH = DDZL.ZLBH '
      'LEFT JOIN XXZLS AS XXZLS ON DDZL.XIEXING = XXZLS.XIEXING '
      
        '         AND DDZL.SHEHAO = XXZLS.SHEHAO AND ZLZLS2.BWBH = XXZLS.' +
        'BWBH --AND ZLZLS2.CLBH = XXZLS.CLBH'
      'where ZLZLS2.ZLBH=:ZLBH and ZLZLS2.MJBH<>'#39'ZZZZZZZZZZ'#39
      
        'GROUP BY ZLZLS2.ZLBH,ZLZLS2.XH,zlzls2.zlbh,zlzls2.bwbh,zlzls2.CL' +
        'BH,XXZLS.CLSL,CLZL.CQDH,XXZLS.LOSS,XXZLS.JGYWSM ,XXZLS.CCQQ,XXZL' +
        'S.CCQZ,ddzl.pairs,DDZL.XieXing ,DDZL.SheHao'
      ') zlzls2'
      
        'GROUP BY ZLZLS2.ZLBH,zlzls2.XH,zlzls2.bwbh , zlzls2.clbh, zlzls2' +
        '.mjbh,zlzls2.clsl,'
      
        #9#9'zlzls2.usage, zlzls2.BWMCY, zlzls2.CLMCY, zlzls2.dwbh, zlzls2.' +
        'LOSS,zlzls2.CQDH, zlzls2.JGYWSM,zlzls2.XieXing,zlzls2.SheHao,zlz' +
        'ls2.CCQQ, zlzls2.CCQZ'
      'ORDER BY  zlzls2.bwbh asc'
      ''
      ''
      'update #Material'
      'set CLMCY=CLMCY+JGYWSM'
      'where JGYWSM is not Null'
      ''
      ' select * from #Material'
      'order by BWBH')
    UpdateObject = UpdateSQL2
    Left = 352
    Top = 217
