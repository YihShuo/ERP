  object qry_RY: TQuery
    DatabaseName = 'DB'
    DataSource = DS_Qc
    SQL.Strings = (
      'select mr.*,lbzls.ywsm as COUNTRY'
      'FROM MaterialQCcheck_RY mr'
      '  LEFT JOIN DDZL do ON mr.RY=do.DDBH'
      '  left join lbzls on do.DDGB=lbzls.lbdh and lbzls.lb='#39'13'#39
      'where No_ID=:No_ID')
    UpdateObject = Upd_RY
    Left = 260
    Top = 407
    ParamData = <
