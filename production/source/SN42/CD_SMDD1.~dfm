  object BWBHQry: TQuery
    DatabaseName = 'DB'
    SQL.Strings = (
      
        'select SMDDS.BWBH,bwzl.YWSM,bwzlvn.VWSM,SMDDS.BWBH as BWBH_DS fr' +
        'om ('
      'select BWBH from CutDispatchZL '
      
        'inner join SMDD on SMDD.YSBH=CutDispatchZL.ZLBH and SMDD.GXLB='#39'C' +
        #39' '
      '         where 1=1 '
      
        '      and SMDD.DDBH in (Select distinct DDBH from SMDD where GXL' +
        'B='#39'C'#39' and  YSBH like '#39'DV2001-0402%'#39' or  YSBH like '#39'DV2003-0021%'#39 +
        ' )'
      'Group by BWBH ) SMDDS'
      'Left join bwzl on bwzl.bwdh=SMDDS.BWBH '
      'Left join bwzlvn on bwzlvn.bwdh=SMDDS.BWBH')
    Left = 94
    Top = 120
    object BWBHQryBWBH: TStringField
      FieldName = 'BWBH'
      FixedChar = True
      Size = 4
    end
    object BWBHQryYWSM: TStringField
      FieldName = 'YWSM'
      FixedChar = True
      Size = 40
    end
    object BWBHQryVWSM: TStringField
      FieldName = 'VWSM'
      FixedChar = True
      Size = 40
    end
    object BWBHQryBWBH_DS: TStringField
      FieldName = 'BWBH_DS'
      FixedChar = True
      Size = 4
    end
  end
  object DS2: TDataSource
    DataSet = BWBHQry
    Left = 94
    Top = 153
  end
