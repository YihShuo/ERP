  object DAOMHQry: TQuery
    DatabaseName = 'DB'
    SQL.Strings = (
      'select DAOMH,Max(Article) as Article,Sum(Qty) as Qty from ('
      
        'select SMDD.PlanDate,BDepartment.DepName,BDepartment.ID,SMDD.DDB' +
        'H,DDZL.DDBH as YSBH,DDZL.Article,XXZL.DAOMH,SMDD.Qty from SMDD '
      'left join DDZL on DDZL.DDBH=SMDD.YSBH '
      
        'left join XXZL on XXZL.XieXing=DDZL.XieXing and XXZL.SheHao=DDZL' +
        '.SheHao '
      'left join BDepartment on BDepartment.ID=SMDD.DepNo '
      'where SMDD.GXLB='#39'C'#39'  and SMDD.DepNO like '#39'%'#39'  '
      
        '      and SMDD.DDBH in (Select distinct DDBH from SMDD where GXL' +
        'B='#39'C'#39' and  YSBH like '#39'Y1802-0466%'#39' or  YSBH like '#39'Y1802-0523%'#39' o' +
        'r  YSBH like '#39'Y1804-0371%'#39' or  YSBH like '#39'Y1804-0934%'#39' or  YSBH ' +
        'like '#39'Y1804-1013%'#39' or  YSBH like '#39'Y1806-0015%'#39' or  YSBH like '#39'Y1' +
        '806-0827%'#39' or  YSBH like '#39'Y1807-0090%'#39' or  YSBH like '#39'Y1902-0084' +
        '%'#39' or  YSBH like '#39'Y1902-0252%'#39' or  YSBH like '#39'Y1902-0290%'#39' or  Y' +
        'SBH like '#39'Y1909-0341%'#39' )'
      ' ) DDZL'
      'Group by DAOMH '
      'order by Qty desc,DAOMH')
    Left = 46
    Top = 112
