        'Select  TSCD.TSID,TSCD.TSBH, TSCD.SBBH , TSCD.Qty, TSCD.XSBH, TS' +
        'CD.NSX, TSCD.Status,TSCD.Label_Tag,TSCD.RFID_Tag,'
      
        '        TSCD.USERID_Tag, TSCD.USERDATE_Tag,TSCD.RKNO, TSCD.LLNO,' +
        ' '
      '        CLZL.ywpm,CLZL.zwpm,BDepartment.DepName '
      'from TSCD '
      'Left join CLZL on CLZL.cldh=TSCD.SBBH '
      'Left join BDepartment on BDepartment.ID=TSCD.DepID '
      'where LLNO in ('#39'20210207720'#39') and LB=2 '
      'and SBBH like '#39'%'#39' '
      'Order by TSCD.Label_Tag,CLZL.YWPM')
    UpdateObject = TSCDUPDet
    Left = 656
    Top = 289
