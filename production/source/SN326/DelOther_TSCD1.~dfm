  object UpdateSQL2: TUpdateSQL
    ModifySQL.Strings = (
      'update BLimits'
      'set'
      '  GSBH = :GSBH,'
      '  USERID = :USERID,'
      '  MKID = :MKID,'
      '  FORMID = :FORMID,'
      '  SUSERID = :SUSERID,'
      '  USERDATE = :USERDATE'
      'where'
      '  GSBH = :OLD_GSBH and'
      '  USERID = :OLD_USERID and'
      '  MKID = :OLD_MKID and'
      '  FORMID = :OLD_FORMID')
    InsertSQL.Strings = (
      'insert into BLimits'
      '  (GSBH, USERID, MKID, FORMID, SUSERID, USERDATE)'
      'values'
      '  (:GSBH, :USERID, :MKID, :FORMID, :SUSERID, :USERDATE)')
    DeleteSQL.Strings = (
      'delete from BLimits'
      'where'
      '  GSBH = :OLD_GSBH and'
      '  USERID = :OLD_USERID and'
      '  MKID = :OLD_MKID and'
      '  FORMID = :OLD_FORMID')
    Left = 464
    Top = 489
