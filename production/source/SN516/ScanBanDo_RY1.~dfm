  object YWCPMas: TQuery
    DatabaseName = 'DB'
    SQL.Strings = (
      'Select DDBH,KCBH,KVBH,SUM(Qty) as Pairs,Status from YWCP'
      'where KCBH='#39'A8'#39' and KVBH='#39'A02'#39' and SB=1'
      'Group by DDBH,KCBH,KVBH,Status '
      'order by DDBH')
    UpdateObject = upMas
    Left = 63
    Top = 168
    object YWCPMasDDBH: TStringField
      FieldName = 'DDBH'
      Origin = 'DB.YWCP.DDBH'
      FixedChar = True
      Size = 15
    end
    object YWCPMasPairs: TIntegerField
      FieldName = 'Pairs'
      Origin = 'DB.YWCP.Qty'
    end
    object YWCPMasKVBH: TStringField
      FieldName = 'KVBH'
      Origin = 'DB.YWCP.KVBH'
      FixedChar = True
      Size = 3
    end
    object YWCPMasKCBH: TStringField
      FieldName = 'KCBH'
      Origin = 'DB.YWCP.KCBH'
      FixedChar = True
      Size = 10
    end
    object YWCPMasStatus: TStringField
      FieldName = 'Status'
      Origin = 'DB.YWCP.Status'
      FixedChar = True
      Size = 10
    end
  end
  object YWCPDet: TQuery
    DatabaseName = 'DB'
    DataSource = DS1
    SQL.Strings = (
      'Select CARTONBAR,KVBH,Qty from YWCP'
      'where KCBH=:KCBH and KVBH=:KVBH and SB=1  and DDBH=:DDBH'
      'order by CARTONBAR')
    UpdateObject = upDet
    Left = 407
    Top = 176
    ParamData = <
      item
        DataType = ftFixedChar
        Name = 'KCBH'
        ParamType = ptUnknown
        Size = 11
      end
      item
        DataType = ftFixedChar
        Name = 'KVBH'
        ParamType = ptUnknown
        Size = 6
      end
      item
        DataType = ftFixedChar
        Name = 'DDBH'
        ParamType = ptUnknown
        Size = 16
      end>
    object YWCPDetCARTONBAR: TStringField
      FieldName = 'CARTONBAR'
      Origin = 'DB.YWCP.CARTONBAR'
      FixedChar = True
    end
    object YWCPDetKVBH: TStringField
