  object Qscbbss: TQuery
    AfterOpen = QscbbssAfterOpen
    DatabaseName = 'DB'
    DataSource = DS2
    SQL.Strings = (
      'select SCBBSS.ProNo,SCBBSS.SCBH,SCBBSS.GXLB,SCBBSS.XXCC,'
      'isnull(SCBBSS.Qty,0) as Qty,SCBBSS.Qty as oldQty,'
      
        ' SCZLS.XXCC as zlXXCC,SCZLS.Qty as zlQty,isnull(okQty,0) as okQt' +
        'y'
      'from SCZLS '
      'left join ( select SCBBSS.XXCC,sum(SCBBSS.Qty) as okQty'
      '       from SCBBSS where SCBBSS.SCBH=:SCBH and SCBBSS.GXLB=:GXLB'
      '       group by SCBBSS.XXCC )   SCBBSSok'
      '       on SCBBSSok.XXCC=SCZLS.XXCC'
      
        'left join (select * from SCBBSS where SCBBSS.ProNo=:ProNo and SC' +
        'BBSS.SCBH=:SCBH and SCBBSS.GXLB=:GXLB ) SCBBSS'
      '       on SCBBSS.XXCC=SCBBSSok.XXCC and SCBBSS.SCBH=SCZLS.SCBH  '
      'where SCZLS.SCBH=:SCBH  '
      'order by SCZLS.XXCC')
    UpdateObject = Upscbbss
    Left = 496
    Top = 362
    ParamData = <
      item
        DataType = ftFixedChar
        Name = 'SCBH'
        ParamType = ptUnknown
        Size = 16
      end
      item
        DataType = ftFixedChar
        Name = 'GXLB'
        ParamType = ptUnknown
        Size = 11
      end
      item
        DataType = ftFixedChar
        Name = 'ProNo'
        ParamType = ptUnknown
        Size = 11
      end
      item
        DataType = ftFixedChar
        Name = 'SCBH'
        ParamType = ptUnknown
      end
      item
        DataType = ftFixedChar
        Name = 'GXLB'
        ParamType = ptUnknown
      end
      item
        DataType = ftFixedChar
        Name = 'SCBH'
        ParamType = ptUnknown
      end>
    object QscbbssProNo: TStringField
      FieldName = 'ProNo'
      FixedChar = True
      Size = 10
    end
    object QscbbssSCBH: TStringField
      FieldName = 'SCBH'
      FixedChar = True
      Size = 15
    end
    object QscbbssGXLB: TStringField
      FieldName = 'GXLB'
      FixedChar = True
      Size = 10
    end
    object QscbbssXXCC: TStringField
      FieldName = 'XXCC'
      FixedChar = True
      Size = 5
    end
    object QscbbssQty: TFloatField
      FieldName = 'Qty'
      DisplayFormat = '##,#0.0'
    end
    object QscbbsszlXXCC: TStringField
      FieldName = 'zlXXCC'
      FixedChar = True
      Size = 5
    end
    object QscbbsszlQty: TFloatField
      FieldName = 'zlQty'
      DisplayFormat = '##,#0.0'
    end
    object QscbbssokQty: TFloatField
      FieldName = 'okQty'
      DisplayFormat = '##,#0.0'
    end
    object QscbbssoldQty: TFloatField
      FieldName = 'oldQty'
      DisplayFormat = '##,#0.0'
    end
  end
