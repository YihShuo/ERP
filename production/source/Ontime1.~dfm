  object UpDet: TUpdateSQL
    ModifySQL.Strings = (
      'update scbbs'
      'set'
      '  ProNo = :ProNo,'
      '  SCBH = :SCBH,'
      '  GXLB = :GXLB,'
      '  Qty = :Qty,'
      '  USERID = :USERID,'
      '  USERDATE = :USERDATE,'
      '  YN = :YN'
      'where'
      '  ProNo = :OLD_ProNo and'
      '  SCBH = :OLD_SCBH and'
      '  GXLB = :OLD_GXLB')
    InsertSQL.Strings = (
      'insert into scbbs'
      '  (ProNo, SCBH, GXLB, Qty, USERID, USERDATE, YN)'
      'values'
      '  (:ProNo, :SCBH, :GXLB, :Qty, :USERID, :USERDATE, :YN)')
    DeleteSQL.Strings = (
      'delete from scbbs'
      'where'
      '  ProNo = :OLD_ProNo and'
      '  SCBH = :OLD_SCBH and'
      '  GXLB = :OLD_GXLB')
    Left = 420
    Top = 393
  end
  object UpMas: TUpdateSQL
    ModifySQL.Strings = (
      'update scbb'
      'set'
      '  ProNo = :ProNo,'
      '  SCDate = :SCDate,'
      '  SCClock = :SCClock,'
      '  SCMinute = :SCMinute,'
      '  SChour = :SChour,'
      '  DepNo = :DepNo,'
      '  USERID = :USERID,'
      '  USERDATE = :USERDATE,'
      '  YN = :YN'
      'where'
      '  ProNo = :OLD_ProNo')
    InsertSQL.Strings = (
      'insert into scbb'
      
        '  (ProNo, SCDate, SCClock, SCMinute, SChour, DepNo, USERID, USER' +
        'DATE, YN)'
      'values'
      
        '  (:ProNo, :SCDate, :SCClock, :SCMinute, :SChour, :DepNo, :USERI' +
        'D, :USERDATE, '
      '   :YN)')
    DeleteSQL.Strings = (
      'delete from scbb'
      'where'
      '  ProNo = :OLD_ProNo')
    Left = 324
    Top = 393
  end
