  object QRepDet: TQuery
    AfterOpen = QRepDetAfterOpen
    DatabaseName = 'DB'
    DataSource = DS1
    SQL.Strings = (
      'select *'
      'from scbbs'
      'where ProNo=:ProNo'
      ' ')
    UpdateObject = UpDet
    Left = 420
    Top = 361
    ParamData = <
      item
        DataType = ftFixedChar
        Name = 'ProNo'
        ParamType = ptUnknown
        Size = 11
      end>
    object QRepDetProNo: TStringField
      FieldName = 'ProNo'
      Origin = 'DB.scbbs.ProNo'
      FixedChar = True
      Size = 10
    end
    object QRepDetSCBH: TStringField
      FieldName = 'SCBH'
      Origin = 'DB.scbbs.SCBH'
      FixedChar = True
      Size = 15
    end
    object QRepDetGXLB: TStringField
      FieldName = 'GXLB'
      Origin = 'DB.scbbs.GXLB'
