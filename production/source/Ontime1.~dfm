  object Upscbbss: TUpdateSQL
    ModifySQL.Strings = (
      'update SCBBSs'
      'set'
      '  ProNo = :ProNo,'
      '  SCBH = :SCBH,'
      '  GXLB = :GXLB,'
      '  XXCC = :XXCC,'
      '  Qty = :Qty'
      'where'
      '  ProNo = :OLD_ProNo and'
      '  SCBH = :OLD_SCBH and'
      '  GXLB = :OLD_GXLB and'
      '  XXCC = :OLD_XXCC')
    InsertSQL.Strings = (
      'insert into SCBBSs'
      '  (ProNo, SCBH, GXLB, XXCC, Qty)'
      'values'
      '  (:ProNo, :SCBH, :GXLB, :XXCC, :Qty)')
    DeleteSQL.Strings = (
      'delete from SCBBSs'
      'where'
      '  ProNo = :OLD_ProNo and'
      '  SCBH = :OLD_SCBH and'
      '  GXLB = :OLD_GXLB and'
      '  XXCC = :OLD_XXCC')
