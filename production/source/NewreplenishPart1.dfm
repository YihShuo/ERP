  object QDet: TQuery
    AfterOpen = QDetAfterOpen
    OnCalcFields = QDetCalcFields
    AutoRefresh = True
    DatabaseName = 'DB'
    DataSource = DS1
    SQL.Strings = (
      
        'select distinct SCBLPART.*,BWZL.YWSM,XXZLS.CLBH,CLZL.YWPM,CLZL.D' +
        'WBH'
      'from SCBLPART'
      'left join BWZL on SCBLPART.BWBH=BWZL.BWDH'
      'left join SCZL on SCZL.SCBH=SCBLPART.SCBH '
      'left join ZLZL on ZLZL.ZLBH=SCZL.ZLBH'
      'left join DDZL on DDZL.DDBH=ZLZL.DDBH'
      
        'left join XXZLS on XXZLS.XieXing=DDZL.XieXing and XXZLS.SheHao=D' +
        'DZL.SheHao and XXZLS.BWBH=SCBLPART.BWBH'
      'left join CLZL on CLZL.CLDH=XXZLS.CLBH'
      'where SCBLPART.SCBH=:SCBH'
      'order by  XXZLS.CLBH,SCBLPART.BWBH,SCBLPART.LR')
    UpdateObject = UpDet
