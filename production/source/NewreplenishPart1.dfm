  object QMas: TQuery
    AfterOpen = QMasAfterOpen
    DatabaseName = 'DB'
    SQL.Strings = (
      'Select SCZL.*  ,XXZL.XieXing,XXZL.SheHao,XXZL.Article, '
      'XXZL.XieMing,DDZL.ShipDate as ETD from SCZL'
      'left join ZLZL on ZLZL.ZLBH=SCZL.ZLBH'
      'left join DDZL on DDZL.DDBH=ZLZL.DDBH'
      
        'left join XXZL on XXZL.XieXing=DDZL.XieXing and XXZL.SheHao=DDZL' +
        '.SheHao'
      'where  SCZL.ZLDW<>'#39'B'#39' and SCZL.DDLB<>'#39'P'#39
      'order by SCZL.ZLBH,  SCZL.SCBH')
    UpdateObject = UpMas
    Left = 320
    Top = 361
