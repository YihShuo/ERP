  object Query2: TQuery
    DatabaseName = 'DB'
    DataSource = DataSource1
    SQL.Strings = (
      
        'SELECT DISTINCT DDZL.DDBH, KT_SOPCutS_GCS.gcbwdh, KT_SOPCutS_GCB' +
        'WD.ywsm, KT_SOPCutS_GCBWD.zwsm FROM DDZL'
      
        'LEFT JOIN KT_SOPCutS_GCS ON KT_SOPCutS_GCS.XieXing = DDZL.XieXin' +
        'g AND KT_SOPCutS_GCS.SheHao = DDZL.SheHao'
      
        'LEFT JOIN KT_SOPCutS_GCBW ON KT_SOPCutS_GCBW.XieXing = KT_SOPCut' +
        'S_GCS.XieXing AND KT_SOPCutS_GCBW.gcbwdh = KT_SOPCutS_GCS.gcbwdh'
      
        'LEFT JOIN KT_SOPCutS_GCBWD ON KT_SOPCutS_GCBWD.gcbwdh = KT_SOPCu' +
        'tS_GCS.gcbwdh'
      'WHERE DDBH = :YSBH'
      'ORDER BY KT_SOPCutS_GCS.gcbwdh')
    Left = 768
    Top = 152
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'YSBH'
        ParamType = ptUnknown
      end>
    object Query2DDBH: TStringField
      FieldName = 'DDBH'
      Size = 15
    end
    object Query2gcbwdh: TStringField
      FieldName = 'gcbwdh'
      Size = 6
    end
    object Query2ywsm: TStringField
      FieldName = 'ywsm'
      Size = 40
    end
    object Query2zwsm: TStringField
      FieldName = 'zwsm'
      Size = 40
    end
  end
