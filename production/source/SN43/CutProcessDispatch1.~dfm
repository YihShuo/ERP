  object CPDDetailUpd: TUpdateSQL
    ModifySQL.Strings = (
      'UPDATE CutDispatchS_GC'
      'Set'
      '  ZLBH=:ZLBH,'
      '  GCBWBH=:GCBWBH,'
      '  SIZE=:SIZE,'
      '  BWBH=:BWBH,'
      '  TmpQty=:TmpQty,'
      '  Qty=:Qty,'
      '  USERID = :USERID,'
      '  USERDATE = :USERDATE'
      'WHERE'
      '  DLNO = :OLD_DLNO  and'
      '  ZLBH = :OLD_ZLBH  and'
      '  GCBWBH = :OLD_GCBWBH  and'
      '  SIZE = :OLD_SIZE  and'
      '  BWBH = :OLD_BWBH')
    InsertSQL.Strings = (
      'INSERT INTO CutDispatchS_GC'
      
        '   (DLNO, ZLBH, GCBWBH, SIZE, BWBH, TmpQty, Qty, USERID, USERDAT' +
        'E, YN)'
      'VALUES'
      
        '   (:DLNO, :ZLBH, :GCBWBH, :SIZE, :BWBH, :TmpQty, :Qty, :USERID,' +
        ' :USERDATE, :YN)')
    DeleteSQL.Strings = (
      'Delete From CutDispatchS_GC'
      'WHERE'
      '  DLNO = :OLD_DLNO  and'
      '  ZLBH = :OLD_ZLBH  and'
      '  GCBWBH = :OLD_GCBWBH  and'
      '  SIZE = :OLD_SIZE  and'
      '  Qty = :OLD_Qty')
    Left = 384
    Top = 376
  end
