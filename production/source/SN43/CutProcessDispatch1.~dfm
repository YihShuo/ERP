  object CPDMasterUpd: TUpdateSQL
    ModifySQL.Strings = (
      'UPDATE CutDispatch_GC'
      'SET'
      '  DLNO = :DLNO,'
      '  GSBH = :GSBH,'
      '  DepID = :DepID,'
      '  PlanDate =:PlanDate,'
      '  Memo =:Memo,'
      '  USERID = :USERID,'
      '  USERDATE = :USERDATE'
      'WHERE'
      '  DLNO = :OLD_DLNO')
    InsertSQL.Strings = (
      'INSERT INTO CutDispatch_GC'
      '(DLNO, GSBH, DepID, PlanDate, Memo, USERID, USERDATE, CFMID, YN)'
      'VALUES'
      
        '(:DLNO, :GSBH, :DepID, :PlanDate, :Memo, :USERID, :USERDATE, :CF' +
        'MID, :YN)')
    DeleteSQL.Strings = (
      'DELETE FROM CutDispatch_GC'
      'WHERE'
      'DLNO = :OLD_DLNO')
    Left = 352
    Top = 376
  end
