    DataSource = DS2
    SQL.Strings = (
      'SELECT * FROM CutDispatchSS_GC'
      'WHERE '
      '  DLNO = :DLNO AND'
      '  ZLBH = :ZLBH AND'
      '  GCBWBH = :GCBWBH AND'
      '  SIZE = :SIZE AND'
      '  BWBH = :BWBH'
      'ORDER BY DDBH')
    UpdateObject = CPDDetailSUpd
    Left = 416
    Top = 344
    ParamData = <
