  object CPDDetailSUpd: TUpdateSQL
    InsertSQL.Strings = (
      'INSERT INTO CutDispatchSS_GC'
      
        '  (DLNO, ZLBH,  DDBH, GCBWBH, SIZE , BWBH,  Qty, USERDATE, USERI' +
        'D, YN)'
      'VALUES'
      
        '  (:DLNO, :ZLBH,  :DDBH, :GCBWBH, :SIZE, :BWBH, :Qty, :USERDATE,' +
        ' :USERID, :YN)')
    DeleteSQL.Strings = (
      'DELETE FROM CutDispatchSS_GC'
      'WHERE'
