  object BLPurMat: TQuery
    DatabaseName = 'DB'
    DataSource = DS1
    SQL.Strings = (
      
        'select CLBH,YWPM,ZWPM,DWBH,SUM(Qty) as Qty,CSBH,ZSYWJC,PayVN,CQD' +
        'H,ZLBH,ARTICLE,ArtName from ('
      
        'select SCBLS.CLBH,CLZL.ywpm,CLZL.zwpm,CLZL.dwbh,SCBLS.Qty,ZSZL.z' +
        'sdh as CSBH,ZSZL.zsywjc,zszl_dev.Zsdh_TW as PayVN,CLZL.cqdh,SCBL' +
        '.ZLBH,XXZL.ARTICLE,XXZL.XieMing as ArtName '
      'from SCBLS'
      'left join SCBL on SCBL.BLNO=SCBLS.BLNO'
      'left join DDZL on DDZL.DDBH=SCBL.ZLBH'
      
        'left join XXZL on XXZL.XieXing=DDZL.XieXing and XXZL.SheHao=DDZL' +
        '.SheHao'
      'left join CLZL on CLZL.cldh=SCBLS.CLBH'
      'left join ZSZL on ZSZL.zsdh=SCBLS.ZSDH '
      
        'left join zszl_dev on ZSZL_DEV.GSBH=:GSBH and ZSZL_DEV.Zsdh=SCBL' +
        'S.ZSDH '
      'where SCBLS.BLNO=:BLNO and SCBLS.ZMLB='#39'N'#39' ) SCBLS'
      
        'Group by CLBH,YWPM,ZWPM,DWBH,CSBH,ZSYWJC,PayVN,CQDH,ZLBH,ARTICLE' +
        ',ArtName '
      'order by CQDH,PayVN')
    Left = 532
    Top = 254
    ParamData = <
      item
        DataType = ftFixedChar
        Name = 'GSBH'
        ParamType = ptUnknown
        Size = 5
      end
      item
        DataType = ftFixedChar
        Name = 'BLNO'
        ParamType = ptUnknown
        Size = 12
      end>
    object BLPurMatCLBH: TStringField
