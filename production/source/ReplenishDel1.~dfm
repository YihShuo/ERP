      'set'
      '  BLNO = :BLNO,'
      '  ZLBH = :ZLBH,'
      '  GXLB = :GXLB,'
      '  GSBH = :GSBH,'
      '  Qty = :Qty,'
      '  BLACC = :BLACC,'
      '  BLDate = :BLDate,'
      '  USERDate = :USERDate,'
      '  USERID = :USERID,'
