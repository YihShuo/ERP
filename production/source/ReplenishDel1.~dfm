  object BLDet: TQuery
    AfterOpen = BLDetAfterOpen
    OnCalcFields = BLDetCalcFields
    OnNewRecord = BLDetNewRecord
    DatabaseName = 'DB'
    DataSource = DS1
    SQL.Strings = (
      
        'select OrdCL.*,SCBLS.Qty ,BLOld.QtyOld,BWZL.YWSM as BWYW,BWZL.ZW' +
        'SM as BWZW'
      
        '          ,SCBLS.BLNO,SCBLS.USPrice,SCBLS.USERDate,SCBLS.USERID,' +
        'SCBLS.YN'
      
        'from (select ZLZLS2.ZLBH,ZLZLS2.BWBH,ZLZLS2.CLBH, sum(ZLZLS2.CLS' +
        'L)   CLSL,'
      
        '           CLZL.YWPM,CLZL.DWBH,isnull(XXBWFLS.DFL,'#39'N'#39') as DFL,MJ' +
        'BH,ZMLB,MAX(CSBH) as ZSDH,MAX(ZSZL.ZSYWJC) as ZSYWJC'
      '        from ZLZLS2'
      '        left join DDZL on DDZl.ZLBH=ZLZLS2.ZLBH'
      
        '        left join XXBWFL on XXBWFL.XieXing=DDZL.XieXing and XXBW' +
        'FL.BWBH=ZLZLS2.BWBH'
      '        left join XXBWFLS on XXBWFLS.FLBH=XXBWFL.FLBH'
      '        left join CLZL on ZLZLS2.CLBH=CLZL.CLDH'
      '        left join ZSZL on ZLZLS2.CSBH=ZSZL.ZSDH'
      '        where ZLZLS2.ZLBH=:ZLBH'
      '                  and ZLZLS2.CLSL<>0'
      '                  and  ZLZLS2.MJBH='#39'ZZZZZZZZZZ'#39
      
        '        group by ZLZLS2.ZLBH,ZLZLS2.CLBH,CLZL.YWPM,CLZL.DWBH ,ZL' +
        'ZLS2.BWBH,XXBWFLS.DFL,MJBH,ZMLB) OrdCL'
      
        'left join   SCBLS on SCBLS.BWBH=OrdCL.BWBH and SCBLS.CLBH=OrdCL.' +
        'CLBH and SCBLS.BLNO=:BLNO '
      'left join BWZL on BWZL.BWDH=OrdCL.BWBH'
      
        'left join (select SCBLS.BWBH,SCBLS.CLBH,sum(SCBLS.Qty) as QtyOld' +
        ' from SCBLS '
      '             left join SCBL on SCBL.BLNO=SCBLS.BLNO '
      '             where SCBL.ZLBH=:ZLBH and SCBLS.BLNO<>:BLNO'
      
        '             group by  SCBLS.BWBH,SCBLS.CLBH) BLOld on BLOld.BWB' +
        'H=OrdCL.BWBH and BLOld.CLBH=OrdCL.CLBH'
      'order by OrdCL.DFL,OrdCL.BWBH,OrdCL.CLBH,OrdCL.ZLBH')
    UpdateObject = Updet
    Left = 377
    Top = 255
    ParamData = <
      item
        DataType = ftFixedChar
        Name = 'ZLBH'
        ParamType = ptUnknown
        Size = 16
      end
      item
        DataType = ftFixedChar
        Name = 'BLNO'
        ParamType = ptUnknown
        Size = 12
      end
      item
        DataType = ftFixedChar
        Name = 'ZLBH'
        ParamType = ptUnknown
      end
      item
        DataType = ftFixedChar
        Name = 'BLNO'
        ParamType = ptUnknown
      end>
    object BLDetZLBH: TStringField
      FieldName = 'ZLBH'
      FixedChar = True
      Size = 15
