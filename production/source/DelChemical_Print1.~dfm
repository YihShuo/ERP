  object DelTot: TQuery
    CachedUpdates = True
    DatabaseName = 'DB'
    DataSource = DelChemical.DS3
    RequestLive = True
    SQL.Strings = (
      'select  KCLLS.CLBH,CLZL.YWPM,CLZL.ZWPM,CLZL.DWBH'
      '         ,sum(tempQty) as Qty,convert(varchar(250),'#39#39') as MEMO'
      'from KCLLS'
      'left join CLZL on CLZL.CLDH=KCLLS.CLBH'
      'where KCLLS.LLNO=:LLNO'
      'group by KCLLS.CLBH,CLZL.YWPM,CLZL.ZWPM,CLZL.DWBH')
    UpdateObject = UpdateSQL1
    Left = 68
    Top = 91
    ParamData = <
      item
        DataType = ftFixedChar
        Name = 'LLNO'
        ParamType = ptUnknown
        Size = 12
      end>
    object DelTotCLBH: TStringField
      FieldName = 'CLBH'
      FixedChar = True
      Size = 15
    end
    object DelTotYWPM: TStringField
      FieldName = 'YWPM'
      FixedChar = True
      Size = 200
    end
    object DelTotZWPM: TStringField
      FieldName = 'ZWPM'
      FixedChar = True
      Size = 200
    end
    object DelTotDWBH: TStringField
      FieldName = 'DWBH'
      FixedChar = True
      Size = 4
    end
    object DelTotQty: TCurrencyField
      FieldName = 'Qty'
      DisplayFormat = '##,#0.00'
    end
    object DelTotMEMO: TStringField
      FieldName = 'MEMO'
      FixedChar = True
      Size = 250
    end
  end
  object UpdateSQL1: TUpdateSQL
    Left = 108
    Top = 91
  end
