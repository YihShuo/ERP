  object XXZL: TQuery
    DatabaseName = 'DB'
    SQL.Strings = (
      
        'select XXZL.XieXing,XXZL.SheHao,XXZL.Article,XXZLSVN.VNBOM,XXZL.' +
        'YSSM,CQDH'
      'from LIY_DD.dbo.XXZL XXZL '
      
        'left join (select distinct XieXing,SheHao,XieXing as VNBOM from ' +
        'XXZLSVN) XXZLSVN'
      
        '           on XXZLSVN.XieXing=XXZL.XieXing and XXZLSVN.SheHao=XX' +
        'ZL.SheHao '
      'where XXZL.XieXing like '#39'%'#39
      'and XXZL.SheHao like '#39'%'#39
      'and XXZL.ARTICLE like '#39'%'#39
      'and CQDH='#39'VR2'#39
      'order by XXZL.XieXing,XXZL.Shehao')
    Left = 148
    Top = 340
