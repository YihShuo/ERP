  object UpXXZLSVN: TUpdateSQL
    ModifySQL.Strings = (
      'update LIY_DD.dbo.XXZLSVN'
      'set'
      '  XieXing = :XieXing,'
      '  SheHao = :SheHao,'
      '  BWBH = :BWBH,'
      '  CLBH = :CLBH,'
      '  CSBH = :CSBH,'
      '  CLSL = :CLSL,'
      '  USERID = :USERID,'
      '  USERDATE = :USERDATE,'
      '  YN = :YN'
      'where'
      '  XieXing = :OLD_XieXing and'
      '  SheHao = :OLD_SheHao and'
      '  BWBH = :OLD_BWBH and'
      '  CQDH=:OLD_CQDH')
    InsertSQL.Strings = (
      'insert into LIY_DD.dbo.XXZLSVN'
      
        '  (XieXing, SheHao, BWBH, CQDH,CLBH, CSBH, CLSL, USERID, USERDAT' +
        'E,YN)'
      'values'
      
        '  (:XieXing, :SheHao, :BWBH,:CQDH, :CLBH, :CSBH, :CLSL, :USERID,' +
        ':USERDATE, :YN)')
    DeleteSQL.Strings = (
      'delete from LIY_DD.dbo.XXZLSVN'
      'where'
      '  XieXing = :OLD_XieXing and'
      '  SheHao = :OLD_SheHao and'
      '  BWBH = :OLD_BWBH and'
      '  CQDH=:OLD_CQDH')
    Left = 436
    Top = 336
  end
