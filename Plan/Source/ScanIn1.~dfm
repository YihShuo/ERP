  object SMDDSS2: TQuery
    DatabaseName = 'DB'
    DataSource = DS2
    SQL.Strings = (
      
        'select DDBH,GXLB,XXCC,sum(CTS*Qty)-sum(okCTS*Qty) as LackQty,sum' +
        '(okcts*qty) as OKqty'
      'from SMDDSS'
      'where DDBH=:DDBH'
      '         and GXLB=:GXLB'
      '         and XXCC=:XXCC'
      'group by DDBH,GXLB,XXCC')
    Left = 376
    Top = 216
    ParamData = <
      item
        DataType = ftFixedChar
        Name = 'DDBH'
        ParamType = ptUnknown
        Size = 16
