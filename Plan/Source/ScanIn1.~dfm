  object SMZL: TQuery
    CachedUpdates = True
    OnCalcFields = SMZLCalcFields
    DatabaseName = 'DB'
    RequestLive = True
    SQL.Strings = (
      
        'select SMZL.USERID,SMZL.USERDATE,SMZL.DepNO,Convert(varchar,SMZL' +
        '.ScanDate) as ScanDate,SMZL.CTS,SMZL.CODEBAR'
      
        '      ,BDepartment.DepName,SMDDSS.DDBH,SMDDSS.GXLB,SMDDSS.XXCC,S' +
        'MDDSS.XH,SMDDSS.Qty,SMZL.CTS as oldCTS'
      'from SMZL '
      'left join BDepartment on BDepartment.ID=SMZL.DepNo '
      'left join SMDDSS on SMDDSS.CODEBAR=SMZL.CODEBAR '
      'where SMZL.DepNO='#39#39
      'order by SMDDSS.GXLB,SMDDSS.DDBH,SMZL.CodeBar '
      '')
    UpdateObject = UpSMZL
    Left = 248
    Top = 216
