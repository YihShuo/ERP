  object UpMas: TUpdateSQL
    ModifySQL.Strings = (
      'update DXBB'
      'set'
      '  ProNo = :ProNo,'
      '  DXDate = :DXDate,'
      '  DepNo = :DepNo,'
      '  USERDATE = :USERDATE,'
      '  USERID = :USERID,'
      '  YN = :YN'
      'where'
      '  ProNo = :OLD_ProNo')
    InsertSQL.Strings = (
      'insert into DXBB'
      '  (ProNo, DXDate, DepNo, USERDATE, USERID, YN)'
      'values'
      '  (:ProNo, :DXDate, :DepNo, :USERDATE, :USERID, :YN)')
    DeleteSQL.Strings = (
      'delete from DXBB'
      'where'
      '  ProNo = :OLD_ProNo')
    Left = 324
    Top = 393
