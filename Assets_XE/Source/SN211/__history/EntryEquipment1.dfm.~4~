  object UpdTSCD: TUpdateSQL
    ModifySQL.Strings = (
      'Update TSCD'
      'set'
      '  TSBH =:TSBH,'
      '  SBBH =:SBBH,'
      '  Qty =:Qty,'
      '  XSBH =:XSBH,'
      '  NSX =:NSX,'
      '  InDate =:InDate,'
      '  DepID =:DepID,'
      '  DepID_Memo =:DepID_Memo,'
      '  USERID =:USERID,'
      '  USERDATE =:USERDATE,'
      '  CFMID =:CFMID,'
      '  CFMID_LL =:CFMID_LL,'
      '  CFMID_RK =:CFMID_RK,'
      '  YN =:YN,'
      '  RKNO=:RKNO,'
      ' Unit_Name=:Unit_Name'
      'where'
      '  TSID =:OLD_TSID')
    InsertSQL.Strings = (
      'insert into TSCD'
      
        '  (TSID , TSBH  , SBBH, Qty, XSBH, NSX, InDate, DepID, DepID_Mem' +
        'o,'
      
        '   USERID ,USERDATE ,YN , CFMID, CFMID_RK, CFMID_LL, RKNO, LB, S' +
        'tatus,Unit_Name)'
      'values'
      
        '  (:TSID , :TSBH  , :SBBH, :Qty, :XSBH, :NSX, :InDate, :DepID, :' +
        'DepID_Memo,'
      
        '    :USERID , :USERDATE , :YN , :CFMID, :CFMID_RK, :CFMID_LL, :R' +
        'KNO, :LB, :Status,:Unit_Name)')
    DeleteSQL.Strings = (
      'delete TSCD'
      'where'
      '  TSID =:OLD_TSID')
    Left = 382
    Top = 390
  end
