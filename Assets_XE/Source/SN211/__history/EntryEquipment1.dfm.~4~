  object EnMas: TQuery
    AfterOpen = EnMasAfterOpen
    AfterScroll = EnMasAfterScroll
    DatabaseName = 'DB'
    SQL.Strings = (
      
        'select TSCD_KCRK.*,BDepartment.DepName,TSCD_ZSZL.ZSJC_YW,TSCD_ZS' +
        'ZL.ZSJC_ZW'
      'from TSCD_KCRK'
      'left join  BDepartment on BDepartment.ID=TSCD_KCRK.DepID '
      'left join TSCD_ZSZL on TSCD_ZSZL.zsdh=TSCD_KCRK.ZSBH '
      'where TSCD_KCRK.RKNO like '#39'%'#39' '
      
        'and convert(smalldatetime,convert(varchar,TSCD_KCRK.USERDATE,111' +
        ')) between '
      #39'2020/03/24'#39
      ' and '
      #39'2020/03/27'#39
      'and TSCD_KCRK.USERID='#39'21191'#39
      'and TSCD_KCRK.LB='#39'NNNNNNNNNN'#39' '
      'order by RKNO DESC')
    UpdateObject = UpMas
    Left = 204
    Top = 315
