  object UpMas: TUpdateSQL
    ModifySQL.Strings = (
      'update TSCD_KCRK'
      'set'
      '  LB =:LB,'
      '  ZSBH=:ZSBH,'
      '  DepID =:DepID,'
      '  SOHIEU =:SOHIEU,'
      '  DOCNO =:DOCNO,'
      '  HDNO =:HDNO,'
      '  DOCDate =:DOCDate,'
      '  HDDate =:HDDate,'
      '  ZSNO =:ZSNO,'
      '  USERID =:USERID,'
      '  USERDATE =:USERDATE,'
      '  CFMID =:CFMID,'
      '  CFMDate =:CFMDate,'
      '  YN =:YN'
      'where'
      '  RKNO =:OLD_RKNO')
    InsertSQL.Strings = (
      'insert into TSCD_KCRK'
      
        '  (RKNO, GSBH, LB, DepID, ZSBH, ZSNO, SOHIEU,DOCNO, HDNO,DOCDate' +
        ', HDDate, USERID, USERDATE, CFMID, CFMDate, YN)'
      'values'
      
        '  (:RKNO, :GSBH, :LB, :DepID, :ZSBH, :ZSNO,:SOHIEU, :DOCNO, :HDN' +
        'O, :DOCDate, :HDDate, :USERID, :USERDATE, :CFMID, :CFMDate, :YN)')
    DeleteSQL.Strings = (
      'delete from TSCD_KCRK'
      'where'
      '  RKNO = :OLD_RKNO')
    Left = 204
    Top = 381
  end
