  object CGMas: TQuery
    AfterOpen = CGMasAfterOpen
    DatabaseName = 'DB'
    SQL.Strings = (
      
        'select TSCD_CGZL.*,BDepartment.DepName,BDepartment.DepMemo,TSCD_' +
        'ZSZL.ZSJC_YW,TSCD_ZSZL.ZSJC_ZW'
      'from TSCD_CGZL'
      'left join  BDepartment on BDepartment.ID=TSCD_CGZL.DepID '
      'left join TSCD_ZSZL on TSCD_ZSZL.zsdh=TSCD_CGZL.ZSBH '
      'where TSCD_CGZL.CGNO like '#39'%'#39' '
      
        'and convert(smalldatetime,convert(varchar,TSCD_CGZL.USERDATE,111' +
        ')) between '
      #39'2020/03/24'#39
      ' and '
      #39'2020/03/27'#39
      'and TSCD_CGZL.USERID='#39'21191'#39
      'order by CGNO DESC')
    UpdateObject = UpMas
