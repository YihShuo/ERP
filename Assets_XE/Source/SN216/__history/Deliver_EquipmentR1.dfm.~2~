  object UpMas: TUpdateSQL
    ModifySQL.Strings = (
      'update TSCD_KCRR'
      'set'
      '  LLNO = :LLNO,'
      '  DepId = :DepId,'
      '  LB = :LB,'
      '  Memo =:Memo,'
      '  USERID = :USERID,'
      '  USERDATE = :USERDATE,'
      '  CFMID = :CFMID,'
      '  CFMDate = :CFMDate,'
      '  YN = :YN'
      'where'
      '  LLNO = :OLD_LLNO')
    InsertSQL.Strings = (
      'insert into TSCD_KCRR'
      
        '  (LLNO, GSBH, DepID, LB, Memo, USERID, USERDATE, CFMID, CFMDate' +
        ', YN)'
      'values'
      
        '  (:LLNO, :GSBH, :DepID, :LB, :Memo, :USERID, :USERDATE, :CFMID,' +
        ' :CFMDate, :YN)'
      '')
    DeleteSQL.Strings = (
      'delete from TSCD_KCRR'
      'where'
      '  LLNO = :OLD_LLNO')
    Left = 212
    Top = 387
  end
