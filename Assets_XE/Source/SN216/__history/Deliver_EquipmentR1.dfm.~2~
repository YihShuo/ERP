  object DelMas: TQuery
    DatabaseName = 'DB'
    SQL.Strings = (
      'select TSCD_KCRR.*,BDepartment.DepName'
      'from TSCD_KCRR'
      'left join BDepartment on TSCD_KCRR.DepID=BDepartment.ID'
      'where TSCD_KCRR.LLNO like '#39'%'#39
      
        'and convert(smalldatetime,convert(varchar,TSCD_KCRR.USERDATE,111' +
        ')) between'
      #39'2018/07/20'#39
      ' and  '
      #39'2018/07/23'#39
      'and BDepartment.GSBH='#39'VA12'#39
      'order by LLNO DESC')
    UpdateObject = UpMas
    Left = 212
    Top = 340
    object DelMasLLNO: TStringField
      DisplayWidth = 11
      FieldName = 'LLNO'
      FixedChar = True
      Size = 11
    end
    object DelMasGSBH: TStringField
      FieldName = 'GSBH'
      FixedChar = True
      Size = 4
    end
    object DelMasDepID: TStringField
      DisplayWidth = 10
      FieldName = 'DepID'
      FixedChar = True
      Size = 10
    end
    object DelMasDepName: TStringField
      DisplayWidth = 13
      FieldName = 'DepName'
      FixedChar = True
      Size = 50
    end
    object DelMasMemo: TStringField
      FieldName = 'Memo'
      FixedChar = True
      Size = 50
    end
    object DelMasLB: TStringField
      DisplayWidth = 11
      FieldName = 'LB'
      FixedChar = True
    end
