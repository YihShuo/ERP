  object DelDet: TQuery
    AfterOpen = DelDetAfterOpen
    DatabaseName = 'DB'
    DataSource = DS1
    SQL.Strings = (
      'select TSCD_KCRRS.LLNO,TSCD_KCRRS.TSID'
      
        ',TSCD_KCRRS.PARAMENT1,TSCD_KCRRS.PARAMENT2,TSCD_KCRRS.PARAMENT3,' +
        'TSCD_KCRRS.PARAMENT4,TSCD_KCRRS.PARAMENT5,TSCD_KCRRS.PARAMENT6'
      
        ',TSCD_KCRRS.VALUE1,TSCD_KCRRS.VALUE2,TSCD_KCRRS.VALUE3,TSCD_KCRR' +
        'S.VALUE4,TSCD_KCRRS.VALUE5,TSCD_KCRRS.VALUE6'
      
        ',TSCD_KCRRS.COLUMN1,TSCD_KCRRS.COLUMN2,TSCD_KCRRS.COLUMN3,TSCD_K' +
        'CRRS.COLUMN4,TSCD_KCRRS.COLUMN5,TSCD_KCRRS.COLUMN6'
      
        ',TSCD.SBBH,TSCD_SB.YWPM,TSCD_SB.ZWPM,TSCD_SB.VWPM,TSCD_SB.DWBH,I' +
        'sNull(JDBDep.DepName,BDepartment.DepName) as DepName,TSCD.DepID_' +
        'Memo,TSCD_KCRRS.USERID,TSCD_KCRRS.USERDATE,TSCD_KCRRS.YN'
      ',TSCD_SB.QUCBH,TSCD_SB.LSBH,TSCD.NSX,TSCD.TSBH,TSCD_KCRRS.Qty'
      'from TSCD_KCRRS'
      'left join TSCD on TSCD.TSID=TSCD_KCRRS.TSID'
      'left join TSCD_SB on TSCD_SB.SBBH = TSCD.SBBH'
      
        'left join TSCD_BDepartment BDepartment on BDepartment.ID=TSCD.De' +
        'pID'
      'left join TSCD_BDepartment JDBDep on JDBDep.ID=TSCD.JDTS'
      'where TSCD_KCRRS.LLNO=:LLNO'
      'order by TSCD_KCRRS.LLNO')
    UpdateObject = UPDet
    Left = 334
    Top = 339
    ParamData = <
      item
        DataType = ftFixedChar
        Name = 'LLNO'
        ParamType = ptUnknown
        Size = 12
      end>
