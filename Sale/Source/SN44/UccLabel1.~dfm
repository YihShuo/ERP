  object YWBZPOS: TQuery
    DatabaseName = 'DB'
    DataSource = DS1
    SQL.Strings = (
      
        'select YWBZPO.DDBH,YWBZPO.XH,sum(ywbzpos.Qty) as Qty,ywbzpos.CTS' +
        ',ywbzpos.CTQ,ywbzpos.CTZ,YWBZPO.gw'
