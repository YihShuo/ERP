  object UpBar: TUpdateSQL
    ModifySQL.Strings = (
      'update YWCP'
      'set'
      '  CARTONBAR = :CARTONBAR,'
      '  DDBH = :DDBH,'
      '  CARTONNO = :CARTONNO,'
      '  XH = :XH,'
      '  Qty = :Qty,'
      '  SB = :SB,'
      '  INCS = :INCS,'
      '  INDATE = :INDATE,'
      '  OUTCS = :OUTCS,'
      '  OUTDATE = :OUTDATE,'
      '  INSPECTCS = :INSPECTCS,'
      '  INSPECTDATE = :INSPECTDATE,'
      '  EXEDATE = :EXEDATE,'
      '  MEMO = :MEMO,'
      '  USERID = :USERID,'
      '  USERDATE = :USERDATE,'
      '  YN = :YN,'
      '  sgw = :sgw'
      'where'
      '  CARTONBAR = :OLD_CARTONBAR')
    InsertSQL.Strings = (
      'insert into YWCP'
      '  (CARTONBAR, DDBH, CARTONNO, XH, Qty, SB, INCS, INDATE, OUTCS, '
      'OUTDATE, '
      
        '   INSPECTCS, INSPECTDATE, EXEDATE, MEMO, USERID, USERDATE, YN, ' +
        'sgw)'
      'values'
      
        '  (:CARTONBAR, :DDBH, :CARTONNO, :XH, :Qty, :SB, :INCS, :INDATE,' +
        ' :OUTCS, '
      
        '   :OUTDATE, :INSPECTCS, :INSPECTDATE, :EXEDATE, :MEMO, :USERID,' +
        ' '
      ':USERDATE, '
      '   :YN, :sgw)')
    DeleteSQL.Strings = (
      'delete from YWCP'
      'where'
      '  CARTONBAR = :OLD_CARTONBAR')
    Left = 632
    Top = 152
  end
