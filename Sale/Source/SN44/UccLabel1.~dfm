        'left join YWBZPO on YWBZPO.ddbh=ywbzpos.ddbh and YWBZPO.xh=ywbzp' +
        'os.xh'
      'where YWBZPO.DDBH in (select DDBH from YWDD where YSBH=:DDBH ) '
      
        'group by YWBZPO.DDBH,YWBZPO.XH,ywbzpos.CTS,ywbzpos.CTQ,ywbzpos.C' +
