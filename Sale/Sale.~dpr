{$R *.res}

var   MutexHandle:Thandle;
begin
  Mutexhandle:=windows.CreateMutex(nil,true,'Sale.exe');
  if   Mutexhandle<>0   then
  begin
      if   getlasterror=windows.ERROR_ALREADY_EXISTS   then
      begin
          closehandle(Mutexhandle);
          halt;
      end;
  end;
  Application.Initialize;
  login:=tlogin.create(application);
  login.Show;
  login.Update;
  //  Application.CreateForm(TLOGIN, LOGIN);
  Application.Title := 'Lai Yih ERP system----Sale';
  Application.CreateForm(TDM2, DM2);
  Application.CreateForm(Tmain, main);
  Application.CreateForm(TUccLabel, UccLabel);
  login.hide;
  login.free;
  Application.Run;

end.
