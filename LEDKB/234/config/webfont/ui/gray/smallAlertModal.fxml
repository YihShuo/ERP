<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>

<Pane fx:id="modalPane" maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="250.0"
	prefWidth="500.0"  xmlns="http://javafx.com/javafx/8.0.111"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<Label fx:id="reminderMessage" layoutX="140.0" layoutY="85.0"
			prefHeight="40.0" prefWidth="290.0" text="确认退出当前程序吗 ?">
			<font>
				<Font size="18.0" />
			</font>
		</Label>
		<Text fx:id="askHelp" layoutX="80.0" layoutY="120.0" strokeType="OUTSIDE"
			strokeWidth="0.0">

		</Text>
		<Text fx:id="closeModal" layoutX="453.0" layoutY="42.0"
			strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="6.0">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</Text>

		<Button  fx:id="confirmBtn" layoutX="164.0" layoutY="170.0"
			mnemonicParsing="false">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</Button>

		<Button fx:id="closeModalBtn"  layoutX="306.0" layoutY="170.0"
			mnemonicParsing="false">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</Button>

	</children>
</Pane>
