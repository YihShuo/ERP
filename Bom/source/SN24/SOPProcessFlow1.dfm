  object SOPCutS_GCQry: TQuery
    AfterOpen = SOPCutS_GCQryAfterOpen
    DatabaseName = 'DB'
    DataSource = DS1
    SQL.Strings = (
      
        'select KT_SOPCutS_GC.*,KT_SOPCutS_GCBWD.ywsm,KT_SOPCutS_GCBWD.zw' +
        'sm,KT_SOPCutS_GCBWD.Qty1Day,KT_SOPCutS_GCBWD.EarlyDay,'
      
        '       KT_SOPCutS_GCBWD.Memo,KT_SOPCutS_GCBWD.ZSDH,ZSZL.ZSJC,IsN' +
        'ull(KT_SOPCutS_GCS.countBWBH,0) as countBWBH'
      'from KT_SOPCutS_GC'
      
        'Left join KT_SOPCutS_GCBWD on KT_SOPCutS_GCBWD.gcbwdh=KT_SOPCutS' +
        '_GC.gcbwdh'
      'Left join ZSZL on ZSZL.ZSDH=KT_SOPCutS_GCBWD.ZSDH'
      
        'left join (Select gcbwdh,COUNT(BWDH) as countBWBH from KT_SOPCut' +
        'S_GCS where KT_SOPCutS_GCS.XieXing=:XieXing and KT_SOPCutS_GCS.S' +
        'heHao=:SheHao   group by gcbwdh ) KT_SOPCutS_GCS on KT_SOPCutS_G' +
        'CS.gcbwdh=KT_SOPCutS_GC.gcbwdh '
      
        'where KT_SOPCutS_GC.XieXing=:XieXing and KT_SOPCutS_GC.SheHao=:S' +
        'heHao'
      'order by  KT_SOPCutS_GC.Levels')
