  object UpdateSQL1: TUpdateSQL
    ModifySQL.Strings = (
      'update ZLZLS2_YM'
      'set '
      '        CLSL=:CLSL,'
      '        USERID=:USERID,'
      '        USERDATE=:USERDATE'
      'WHERE '
      '   ZLBH=:OLD_ZLBH AND'
      '   BWBH=:OLD_BWBH AND'
      '   CLBH=:OLD_CLBH AND'
      '   SIZE=:OLD_SIZE AND'
      '  YN=:OLD_YN'
      ''
      '')
    InsertSQL.Strings = (
      'Insert into ZLZLS2_YM'
      
        '   (ZLBH, XH ,BWBH, CSBH, MJBH, CLBH, ZMLB, SIZE, CLSL, USAGE, U' +
        'SERID, USERDATE, YN)'
      'Values'
      
        '   (:ZLBH, :XH ,:BWBH, :CSBH, :MJBH, :CLBH, :ZMLB, :SIZE, :CLSL,' +
        ' :USAGE, :USERID, :USERDATE, :YN)')
    DeleteSQL.Strings = (
      'Delete From ZLZLS2_YM'
      'WHERE'
      '   ZLBH=:OLD_ZLBH AND'
      '   BWBH=:OLD_BWBH AND'
      '   CLBH=:OLD_CLBH AND'
      '   SIZE=:OLD_SIZE AND'
      '   YN=:OLD_YN')
    Left = 176
    Top = 216
  end
