  object UpdateQry: TQuery
    DatabaseName = 'DB'
    SQL.Strings = (
      'select ZLZLS2.*, BWZL.ywsm BWMC,CLZL.YWPM as CLMC,'
      '       CLZL.DWBH,ZSZL.ZSYWJC AS ZSMC,DDZL.ARTICLE'
      'FROM ZLZLS2 LEFT JOIN BWZL ON BWZL.BWDH=ZLZLS2.BWBH'
      'LEFT JOIN CLZL ON ZLZLS2.CLBH=CLZL.CLDH'
      'LEFT JOIN ZSZL ON ZSZL.ZSDH=ZLZLS2.CSBH'
      'LEFT JOIN DDZL ON DDZL.DDBH=ZLZLS2.ZLBH'
      'WHERE ZLZLS2.ZLBH='#39'SD1511-108'#39'AND ZLZLS2.CLBH='#39'L090000993'#39
      'order by ZLZLS2.ZLBH, ZLZLS2.BWBH')
    Left = 176
    Top = 248
  end
  object KCLLSQry: TQuery
    DatabaseName = 'DB'
    DataSource = DataSource1
    SQL.Strings = (
      'select SCBH,CLBH,MJBH,IsNull(Sum(Qty),0) as Qty from ('
      
        '            select   KCLLS.SCBH, KCLLS.CLBH,'#39'ZZZZZZZZZZ'#39' as MJBH' +
        ', sum(KCLLS.Qty) as Qty'
      '            from KCLLS'
      '            Left Join KCLL on KCLL.LLNO=KCLLS.LLNO '
      
        '            where  KCLLS.SCBH like '#39'Y1712-0001%'#39'  and KCLLS.CLBH' +
        ' like '#39'C030002309%'#39'  '
      '                 and KCLL.GSBH='#39'VA12'#39' '
      '                 and KCLL.SCBH<>'#39'JJJJJJJJJJ'#39' '
      '            Group by KCLLS.SCBH, KCLLS.CLBH'
      '            Union all'
      
        '            select JGZLSS.ZLBH as SCBH,JGZLS.ZMLB as CLBH,JGZLS.' +
        'CLBH as MJBH,Sum(JGZLS.Qty*JGZLSS.Qty) as Qty from JGZLS'
      
        '            Left Join JGZLSS on JGZLS.JGNO=JGZLSS.JGNO and JGZLS' +
        '.CLBH=JGZLSS.CLBH '
      '            Left Join JGZL  on JGZL.JGNO=JGZLS.JGNO '
      
        '            where JGZLSS.ZLBH like '#39'Y1712-0001%'#39' and JGZLS.ZMLB ' +
        'like '#39'C030002309%'#39' and JGZLS.CLBH like '#39'%'#39'  '
      '                  and JGZL.GSBH='#39'VA12'#39' '
      
        '                  and convert(smalldatetime,convert(varchar,JGZL' +
        '.CFMDate1,111))<'#39'2017/09/01'#39'  '
      '            Group by JGZLSS.ZLBH,JGZLS.ZMLB,JGZLS.CLBH'
      '            Union all'
      
        '            select   KCLLS.SCBH, KCLLS.CLBH,KCLLS.DFL as MJBH, s' +
        'um(KCLLS.Qty) as Qty'
      '            from KCLLS'
      '            Left Join KCLL on KCLL.LLNO=KCLLS.LLNO '
      
        '            where  KCLLS.SCBH like '#39'Y1712-0001%'#39'  and KCLLS.CLBH' +
        ' like '#39'C030002309%'#39'  '
      '                 and KCLL.GSBH='#39'VA12'#39' '
      '                 and KCLL.SCBH='#39'JJJJJJJJJJ'#39' '
      '            Group by KCLLS.SCBH, KCLLS.CLBH,KCLLS.DFL'
      '           ) KCLLS group by SCBH,CLBH,MJBH')
    Left = 528
    Top = 216
