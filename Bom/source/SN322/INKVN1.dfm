    SQL.Strings = (
      
        'select  m.ZLBH,m.DDBH,d.Pairs as Qty,d.XieXing,d.SheHao,d.Articl' +
        'e,'
      
        'd.ShipDate,b.CCGB as XXCC,b.XieMing,d.DDZT,d.CCGB as DDCC ,KFZL.' +
        'KFJC,XXZLSVN.VNBOM,SCZL.SCBH,zlzls2_VN.USERDATE as CalDate,SCZLD' +
        'ate.ADate as PlanDate'
      'from zlzl m left join DDZL d on m.DDBH=d.DDBH'
      'left join XXZL b on d.XieXing=b.XieXing and d.SheHao=b.SheHao'
      'left join SCZLDate on SCZLDate.ZLBH=m.DDBH '
      'left join kfzl on KFZl.KFDH=d.KHBH'
      
        'left join (select distinct  XieXing ,SheHao, XieXing as VNBOM fr' +
        'om XXZLSVN ) XXZLSVN '
      
        '            on XXZLSVN.XieXing=b.XieXing and XXZLSVN.SheHao=b.Sh' +
        'eHao '
      
        'left join (select distinct SCBH from SCZL ) SCZL on SCZL.SCBH=m.' +
        'ZLBH '
      
        'left join (select distinct ZLBH, USERDATE from zlzls2_VN)zlzls2_' +
        'VN on zlzls2_VN.ZLBH=m.DDBH'
      'where m.ZLBH like '#39'%%'#39
      
        'and convert(smalldatetime,convert(varchar,SCZLDate.ADate,111)) b' +
        'etween '
      #39'2018/01/27'#39' and '#39'2018/02/26'#39
      'and d.GSBH='#39'VA12'#39
      'order by m.ZLBH'
      '')
