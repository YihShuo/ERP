  object UpdateSQL1: TUpdateSQL
    ModifySQL.Strings = (
      'update CostingPriceList'
      'set'
      '  PricingSeason = :PricingSeason,'
      '  SKU = :SKU,'
      '  Remark = :Remark,'
      '  CheckID = :CheckID,'
      '  CheckDate = :CheckDate,'
      '  FOB = :FOB,'
      '  KFCQ = :KFCQ,'
      '  KHDH = :KHDH,'
      '  Style_Number = :Style_Number,'
      '  Round = :Round,'
      '  Style_Name = :Style_Name,'
      '  Division = :Division,'
      '  Colorway = :Colorway,'
      '  Color_Code = :Color_Code'
      'where'
      '  NO = :OLD_NO and'
      '  PricingSeason = :OLD_PricingSeason')
    InsertSQL.Strings = (
      'insert into CostingPriceList'
      
        '  (PricingSeason, SKU, Remark, CheckID, CheckDate, FOB, KFCQ, KH' +
        'DH, '
      'Style_Number, '
      '   Round, Style_Name, Division, Colorway, Color_Code)'
      'values'
      
        '  (:PricingSeason, :SKU, :Remark, :CheckID, :CheckDate, :FOB, :K' +
        'FCQ, :KHDH, '
      
        '   :Style_Number, :Round, :Style_Name, :Division, :Colorway, :Co' +
        'lor_Code)')
    DeleteSQL.Strings = (
      'delete from CostingPriceList'
      'where'
      '  NO = :OLD_NO and'
      '  PricingSeason = :OLD_PricingSeason')
    Left = 44
    Top = 304
