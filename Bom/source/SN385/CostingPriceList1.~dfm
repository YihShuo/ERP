    InsertSQL.Strings = (
      'insert into CostingPriceList'
      
        '  (No, PricingSeason, SKU, ModelDescription, Remark, UpperMatDet' +
        'ail, CheckID, '
      
        '   CheckDate, AvailableDate, AvailableBy, SendCTSDate, FOB, Mate' +
        'rial, Labor, '
      
