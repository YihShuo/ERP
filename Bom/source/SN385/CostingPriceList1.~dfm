  object UpdateSQL2: TUpdateSQL
    ModifySQL.Strings = (
      'update CostingPriceList'
      'set'
      '  SKU = :SKU,'
      '  ModelDescription = :ModelDescription,'
      '  Remark = :Remark,'
      '  UpperMatDetail = :UpperMatDetail,'
      '  CheckID = :CheckID,'
      '  CheckDate = :CheckDate,'
      '  AvailableDate = :AvailableDate,'
      '  AvailableBy = :AvailableBy,'
      '  SendCTSDate = :SendCTSDate,'
      '  FOB = :FOB,'
      '  C2B_RL = :C2B_RL,'
      '  Material = :Material,'
      '  Labor = :Labor,'
      '  OH = :OH,'
