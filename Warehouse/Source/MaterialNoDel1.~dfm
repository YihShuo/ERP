  object Query1: TQuery
    AfterOpen = Query1AfterOpen
    DatabaseName = 'DB'
    SQL.Strings = (
      'select KCSMCL.*,CLZL.YWPM,CLZL.DWBH,DDZL.YN as Me,YWCP.SB'
      'from KCSMCL'
      'left join CLZL on CLZL.CLDH=KCSMCL.CLBH'
      'left join DDZL on DDZL.ZLBH=KCSMCL.ZLBH '
      
        'left join (select distinct YWDD.YSBH as DDBH,'#39'NoOk'#39' as SB from Y' +
        'WCP'
      '             left join YWDD on YWDD.DDBH=YWCP.DDBH'
      '             where InDate is null ) YWCP on YWCP.DDBH=DDZL.DDBH'
      'where KCSMCL.ZLBH like '#39'%'#39
      'and KCSMCL.CLBH like '#39'%'#39
      'and KCSMCL.YN='#39'1'#39
      'and DDZL.YN='#39'5'#39
      'order by KCSMCL.CLBH,KCSMCL.ZLBH,KCSMCL.XH'
      '')
    UpdateObject = UpSQL1
    Left = 152
    Top = 96
    object Query1ZLBH: TStringField
