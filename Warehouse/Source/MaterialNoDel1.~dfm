  object UpSQL1: TUpdateSQL
    ModifySQL.Strings = (
      'update KCSMCL'
      'set'
      '  USRDATE2 = :USRDATE2,'
      '  USERID2 = :USERID2,'
      '  YN = :YN'
      'where'
      '  ZLBH = :OLD_ZLBH and'
      '  CLBH = :OLD_CLBH and'
      '  DFL = :OLD_DFL and'
      '  XH = :OLD_XH and'
      '  Qty = :OLD_Qty and'
      '  MATCODE = :OLD_MATCODE and'
      '  DepID = :OLD_DepID and'
      '  USERDATE = :OLD_USERDATE and'
      '  USERID = :OLD_USERID and'
      '  USRDATE2 = :OLD_USRDATE2 and'
      '  USERID2 = :OLD_USERID2 and'
      '  YN = :OLD_YN')
    InsertSQL.Strings = (
      'insert into KCSMCL'
      '  (USRDATE2, USERID2, YN)'
      'values'
      '  (:USRDATE2, :USERID2, :YN)')
    DeleteSQL.Strings = (
      'delete from KCSMCL'
      'where'
      '  ZLBH = :OLD_ZLBH and'
      '  CLBH = :OLD_CLBH and'
      '  DFL = :OLD_DFL and'
      '  XH = :OLD_XH and'
      '  Qty = :OLD_Qty and'
      '  MATCODE = :OLD_MATCODE and'
      '  DepID = :OLD_DepID and'
      '  USERDATE = :OLD_USERDATE and'
      '  USERID = :OLD_USERID and'
      '  USRDATE2 = :OLD_USRDATE2 and'
      '  USERID2 = :OLD_USERID2 and'
      '  YN = :OLD_YN')
    Left = 184
    Top = 96
  end
