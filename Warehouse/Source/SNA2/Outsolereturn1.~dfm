  object KCLLSS: TQuery
    OnCalcFields = KCLLSSCalcFields
    DatabaseName = 'DB'
    DataSource = DS4
    SQL.Strings = (
      
        'select OSRKSS.*,IsNull(OSLLSS.Qty,0) -IsNull(RK.RKQty,0) as LLQt' +
        'y  from OSRKSS'
      
        'left join OSLLSS on OSLLSS.LLNO=:ZLBH  and OSLLSS.CLBH=:CLBH  an' +
        'd OSRKSS.SIZ=OSLLSS.SIZ'
      
        'left join (select A.CLBH,A.SIZ,Sum(A.QTY) as RKQty  from OSRKSS ' +
        'A,OSRKS B where A.RKNO=B.RKNO and A.CLBH=B.CLBH and B.ZLBH=:ZLBH' +
        ' and B.CLBH=:CLBH and B.RKNO<>:RKNO Group by A.CLBH,A.SIZ)'
      '          RK on RK.CLBH=OSLLSS.CLBH and RK.SIZ=OSLLSS.SIZ'
      'where OSRKSS.RKno=:RKNO and OSRKSS.clbh=:CLBH')
    UpdateObject = UpdateSQL1
    Left = 793
    Top = 379
    ParamData = <
