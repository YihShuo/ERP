  object Query1: TQuery
    DatabaseName = 'DB'
    Left = 88
    Top = 376
  end
  object DelMas: TQuery
    AfterOpen = DelMasAfterOpen
    DatabaseName = 'DB'
    SQL.Strings = (
      'select LTRK.*,BDepartment.DepName'
      'from LTRK'
      'left join BDepartment on LTRK.DepID=BDepartment.ID'
      'where LTRK.SB='#39'L'#39' '
      
        '    and convert(smalldatetime,convert(varchar,LTRK.USERDATE,111)' +
        ') between '
      #39'2016/08/08'#39' and  '#39'2016/08/09'#39
      'and LTRK.USERID='#39'21083'#39
      'order by LTRK.RKNO DESC')
    UpdateObject = UpMas
    Left = 420
    Top = 380
    object strngfldDelMasRKNO: TStringField
      FieldName = 'RKNO'
      Origin = 'DB.LTRK.RKNO'
      FixedChar = True
      Size = 13
    end
    object strngfldDelMasGSBH: TStringField
      FieldName = 'GSBH'
      Origin = 'DB.LTRK.GSBH'
      FixedChar = True
      Size = 4
    end
    object strngfldDelMasCKBH: TStringField
      FieldName = 'CKBH'
      Origin = 'DB.LTRK.CKBH'
      FixedChar = True
      Size = 4
    end
    object strngfldDelMasZSNO: TStringField
      FieldName = 'ZSNO'
      Origin = 'DB.LTRK.ZSNO'
      FixedChar = True
    end
    object strngfldDelMasDepID: TStringField
      FieldName = 'DepID'
      Origin = 'DB.LTRK.DepID'
      FixedChar = True
      Size = 10
    end
    object strngfldDelMasDepName: TStringField
      FieldName = 'DepName'
      Origin = 'DB.BDepartment.DepName'
      FixedChar = True
      Size = 50
    end
    object strngfldDelMasUSERID: TStringField
