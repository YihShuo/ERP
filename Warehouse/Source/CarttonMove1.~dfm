  object SCSMCK: TQuery
    OnCalcFields = SCSMCKCalcFields
    DatabaseName = 'DB'
    SQL.Strings = (
      
        'select SCSMCK.DDBH,count(SCSMCK.CTNO) as CTQty,sum(SCSMRK.Qty) a' +
        's Qty,XXZL.XieXing,XXZL.Shehao,XXZL.Article,'
      
        'XXZL.XieMing,DDZL.Pairs as DDQty,DDZL.shipdate,SCZLDate.CTQty as' +
        ' CTDD'
      'from SCSMCK '
      
        'left join SCSMRK on SCSMCK.DDBH=SCSMRK.DDBH and SCSMCK.CTNO=SCSM' +
        'RK.CTNO '
      'left join DDZL on SCSMCK.DDBH=DDZL.DDBH '
      
        'left join XXZl on XXZL.XieXing=DDZl.XieXing and XXZL.SheHao=DDZL' +
        '.SheHao '
      'left join SCZLDate on SCZLDate.ZLBH=DDZL.ZLBH '
      
        'where Convert(smalldatetime,convert(varchar,SCSMCK.USERDate,111)' +
        ') between '#39'2008/03/01'#39'  and '#39'2008/03/30'#39
      
        'group by  SCSMCK.DDBH,XXZL.XieXing,XXZL.Shehao,XXZL.Article,SCZL' +
        'Date.CTQty,XXZL.XieMing,DDZL.Pairs,DDZL.shipdate'
      'order by SCSMCK.DDBH ')
    Left = 184
    Top = 144
    object SCSMCKDDBH: TStringField
      FieldName = 'DDBH'
