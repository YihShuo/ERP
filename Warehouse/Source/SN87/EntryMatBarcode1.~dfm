  object UpdKCKRS: TUpdateSQL
    ModifySQL.Strings = (
      'Update'
      '   KCRKScan'
      'Set'
      '   USERID =:USERID,'
      '   USERDATE =:USERDATE,'
      '   YN =:YN'
      'where'
      '   RKNO = :OLD_RKNO  and'
      '   CLBH = :OLD_CLBH  and'
      '   XH =:OLD_XH and'
      '   SB = :OLD_SB')
    InsertSQL.Strings = (
      'Insert into KCRKScan'
      '    (RKNO , CLBH, XH, SB, Qty, USERID, USERDATE, YN)'
      'Values'
      '    (:RKNO ,:CLBH , :XH, :SB , :Qty,  :USERID, :USERDATE, :YN)')
    DeleteSQL.Strings = (
      'Delete'
      'From'
      '   KCRKScan'
      'where'
      '   RKNO = :OLD_RKNO  and'
      '   CLBH = :OLD_CLBH  and'
      '   XH =:OLD_XH and'
      '   SB = :OLD_SB')
    Left = 411
    Top = 268
  end
  object DS2: TDataSource
    DataSet = KCRKSScan
    Left = 900
    Top = 284
  end
  object KCRKSScan: TQuery
    AfterOpen = KCRKSScanAfterOpen
    DatabaseName = 'DB'
    DataSource = DS1
    SQL.Strings = (
      'select KCRKScan.*,KCRK_OM.LLQty from KCRKScan'
      'left join ('
      '  select RKNO,CLBH,XH,Sum(IsNull(Qty,0)) as LLQty from KCRKScan'
      '  where  YN<>5 and SB=2 '
      '  Group by RKNO,CLBH,XH'
      
        '  ) KCRK_OM on KCRK_OM.RKNO=KCRKScan.RKNO and  KCRK_OM.CLBH=KCRK' +
        'Scan.CLBH and KCRK_OM.XH=KCRKScan.XH'
      'where KCRKScan.RKNO =:RKNO'
      '      and KCRKScan.CLBH=:CLBH'
      '      and KCRKScan.SB=1')
    UpdateObject = UpdateKCKRScan
    Left = 900
    Top = 316
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'RKNO'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'CLBH'
        ParamType = ptUnknown
      end>
    object KCRKSScanRKNO: TStringField
      FieldName = 'RKNO'
      FixedChar = True
      Size = 11
    end
    object KCRKSScanCLBH: TStringField
      FieldName = 'CLBH'
      FixedChar = True
      Size = 15
    end
    object KCRKSScanXH: TStringField
      FieldName = 'XH'
      FixedChar = True
      Size = 3
    end
    object KCRKSScanSB: TStringField
      FieldName = 'SB'
      FixedChar = True
      Size = 1
    end
    object KCRKSScanQty: TFloatField
      FieldName = 'Qty'
    end
    object KCRKSScanUSERDATE: TDateTimeField
      FieldName = 'USERDATE'
    end
    object KCRKSScanUSERID: TStringField
      FieldName = 'USERID'
      FixedChar = True
      Size = 10
    end
    object KCRKSScanYN: TStringField
      FieldName = 'YN'
      FixedChar = True
      Size = 1
    end
    object KCRKSScanLLQty: TFloatField
      FieldName = 'LLQty'
    end
  end
  object UpdateKCKRScan: TUpdateSQL
    ModifySQL.Strings = (
      'Update'
      '   KCRKScan'
      'Set'
      '   Qty=:Qty,'
      '   USERID =:USERID,'
      '   USERDATE =:USERDATE,'
      '   YN =:YN'
      'where'
      '   RKNO = :OLD_RKNO  and'
      '   CLBH = :OLD_CLBH  and'
      '   XH =:OLD_XH and'
      '   SB = :OLD_SB')
    InsertSQL.Strings = (
      'Insert into KCRKScan'
      '    (RKNO , CLBH, XH, SB, Qty, USERID, USERDATE, YN)'
      'Values'
      '    (:RKNO ,:CLBH , :XH, :SB , :Qty,  :USERID, :USERDATE, :YN)')
    DeleteSQL.Strings = (
      'Delete'
      'From'
      '   KCRKScan'
      'where'
      '   RKNO = :OLD_RKNO  and'
      '   CLBH = :OLD_CLBH  and'
      '   XH =:OLD_XH and'
      '   SB = :OLD_SB')
    Left = 899
    Top = 348
  end
  object tmpQry: TQuery
