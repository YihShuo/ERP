  object KCRKS: TQuery
    CachedUpdates = True
    DatabaseName = 'DB'
    RequestLive = True
    SQL.Strings = (
      
        'select KCRKS.RKNO,KCRK.ZSBH,ZSZL.ZSJC,KCRKS.CLBH,CLZL.YWPM,Sum(K' +
        'CRKS.Qty) as Qty,100 as PackQty,Max(KCRK_IM.BQty) as BQty ,'
      
        '    cast((select B.Article + '#39','#39'from KCRKS A ,DDZL B  where B.DD' +
        'BH=A.CGBH and A.RKNO=KCRKS.RKNO group by Article for XML Path ('#39 +
        #39')) as varchar(500)) as Article,'
      
        '    cast((select B.BuyNo + '#39','#39'from KCRKS A ,DDZL B  where B.DDBH' +
        '=A.CGBH and A.RKNO=KCRKS.RKNO group by BuyNo for XML Path ('#39#39')) ' +
        'as varchar(500)) as BuyNo  '
      'from KCRKS'
      'left join KCRK on KCRK.RKNO=KCRKS.RKNO'
      'left join CLZL on CLZL.CLDH=KCRKS.CLBH'
      'left join ZSZL on KCRK.ZSBH=ZSZL.ZSDH'
      'left join ('
      '  select RKNO,CLBH,Sum(Qty) as BQty from KCRKScan'
      '  where  YN<>5 and SB=1 '
      '  Group by RKNO,CLBH'
      
        '  ) KCRK_IM on KCRK_IM.RKNO=KCRK.RKNO and  KCRK_IM.CLBH=KCRKS.CL' +
        'BH'
      'where KCRKS.RKNO like '#39'20171000002%'#39
      
        '    and convert(smalldatetime,convert(varchar,KCRK.USERDATE,111)' +
        ') between '
      '          '#39'2017/01/23'#39' and '#39'2017/10/25'#39
      'Group by KCRKS.RKNO,KCRK.ZSBH,ZSZL.ZSJC,KCRKS.CLBH,CLZL.YWPM')
    UpdateObject = UpdKCKRS
    Left = 412
    Top = 236
    object KCRKSRKNO: TStringField
      FieldName = 'RKNO'
      FixedChar = True
      Size = 11
    end
    object KCRKSZSBH: TStringField
      FieldName = 'ZSBH'
      FixedChar = True
      Size = 6
    end
    object KCRKSZSJC: TStringField
      FieldName = 'ZSJC'
      FixedChar = True
    end
    object KCRKSCLBH: TStringField
      FieldName = 'CLBH'
      FixedChar = True
      Size = 15
    end
    object KCRKSYWPM: TStringField
      FieldName = 'YWPM'
      FixedChar = True
      Size = 200
    end
    object KCRKSQty: TCurrencyField
      FieldName = 'Qty'
      DisplayFormat = '#0.00'
    end
    object KCRKSPackQty: TIntegerField
      FieldName = 'PackQty'
    end
    object KCRKSBQty: TFloatField
      FieldName = 'BQty'
    end
    object KCRKSArticle: TStringField
      FieldName = 'Article'
      FixedChar = True
      Size = 255
    end
    object KCRKSBuyNo: TStringField
      FieldName = 'BuyNo'
