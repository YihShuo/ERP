  object Query1: TQuery
    DatabaseName = 'DB'
    SQL.Strings = (
      'select CLZL.*,KCZL.CKBH,KCZL.KCBH,KCZL.KCMC'
      'from clzl'
      'left join KCZLS on KCZLS.CLBH=CLZL.CLDH '
      'left join KCZL on KCZL.KCBH=KCZLS.KCBH '
      'left join KCCK on KCCK.CKBH=KCZL.CKBH and KCCK.GSBH='#39'VTX'#39
      'order by CLZL.CLDH')
