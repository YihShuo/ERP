  object SCSMYW: TQuery
    DatabaseName = 'DB'
    DataSource = InspectorRep.DS1
    SQL.Strings = (
      'select SCSMYW.*,SCSMRK.Qty'
      'from SCSMYW'
      
        'left join SCSMRK on SCSMRK.DDBH=SCSMYW.DDBH and SCSMRK.CTNO=SCSM' +
        'YW.CTNO'
      'where SCSMYW.DDBH=:DDBH'
      'order by SCSMYW.CTNO')
