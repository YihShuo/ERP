  object UpKCSM: TUpdateSQL
    ModifySQL.Strings = (
      'update KCSMCL'
      'set'
      '  USRDATE2 = :USRDATE2,'
      '  USERID2 = :USERID2,'
      '  YN = :YN'
      'where'
      '  ZLBH = :OLD_ZLBH and'
      '  CLBH = :OLD_CLBH and'
      '  DFL = :OLD_DFL and'
      '  XH = :OLD_XH')
    InsertSQL.Strings = (
      'insert into KCSMCL'
      '  (USRDATE2, USERID2, YN)'
      'values'
      '  (:USRDATE2, :USERID2, :YN)')
    DeleteSQL.Strings = (
      'delete from KCSMCL'
      'where'
      '  ZLBH = :OLD_ZLBH and'
      '  CLBH = :OLD_CLBH and'
      '  DFL = :OLD_DFL and'
      '  XH = :OLD_XH')
    Left = 112
    Top = 232
  end
