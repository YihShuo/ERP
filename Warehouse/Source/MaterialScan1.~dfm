  object KCSM: TQuery
    CachedUpdates = True
    DatabaseName = 'DB'
    RequestLive = True
    SQL.Strings = (
      'select KCSMCL.*,CLZL.YWPM,CLZL.DWBH,BDepartment.DepName'
      'from KCSMCL '
      'left join CLZL on CLZL.CLDH=KCSMCL.CLBH'
      'left join BDepartment on BDepartment.ID=KCSMCL.DepID'
      'where KCSMCL.CLBH='#39'AAAAAA'#39)
    UpdateObject = UpKCSM
    Left = 80
    Top = 232
