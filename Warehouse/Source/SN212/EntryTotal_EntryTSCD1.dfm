      '  CFMID_RK =:CFMID_RK,'
      '  YN =:YN'
      'where'
      '  TSID =:OLD_TSID')
    InsertSQL.Strings = (
      'insert into TSCD'
      
        '  (TSID , TSBH  , SBBH, Qty, XSBH, NSX, InDate, DepID, DepID_Mem' +
        'o, Status,'
      
        '   USERID ,USERDATE ,CFMID, CFMDATE, YN, CFMID_LL, CFMDATE_LL, L' +
        'LNO, CFMID_RK, CFMDATE_RK, RKNO, LB)'
      'values'
      
