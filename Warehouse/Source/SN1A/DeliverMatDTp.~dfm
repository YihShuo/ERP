  object UpMas: TUpdateSQL
    ModifySQL.Strings = (
      'update CCLD'
      'set'
      '  CLMC = :CLMC,'
      '  LCBM = :LCBM,'
      '  LB = :LB,'
      '  SCBM = :SCBM,'
      '  SYBM = :SYBM,'
      '  CSHJS = :CSHJS,'
      '  GSBH = :GSBH,'
      '  USERID = :USERID,'
      '  USERDATE = :USERDATE'
      'where'
      '  CCLDNO= :OLD_CCLDNO')
    InsertSQL.Strings = (
      'insert into CCLD'
      
        '  (CCLDNO, DDBH, CLMC, LB, LCBM, SCBM, SYBM,CSHJS,GSBH, USERID,U' +
        'SERDATE )'
      'values'
      
        '  (:CCLDNO, :DDBH, :CLMC, :LB, :LCBM, :SCBM, :SYBM, :CSHJS, :GSB' +
        'H, :USERID, :USERDATE)')
    DeleteSQL.Strings = (
      'delete from CCLD'
      'where'
      '  CCLDNO= :OLD_CCLDNO')
    Left = 322
    Top = 234
  end
