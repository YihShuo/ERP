  object CCLDMas: TQuery
    AfterOpen = CCLDMasAfterOpen
    BeforeScroll = CCLDMasBeforeScroll
    DatabaseName = 'DB'
    SQL.Strings = (
      'select CCLD.*,'
      
        '(select top 1 B1.DepName from BDepartment B1 where (CCLD.LCBM=B1' +
        '.ID)) as LCBMNM,'
      
        '(select top 1 B2.DepName from BDepartment B2 where (CCLD.SCBM=B2' +
        '.ID)) as SCBMNM,'
      
        '(select top 1 B3.DepName from BDepartment B3 where (CCLD.SYBM=B3' +
        '.ID)) as SYBMNM'
      'from CCLD '
      'order by CCLDNO')
    UpdateObject = UpMas
    Left = 320
    Top = 200
  end
  object CCLDDet: TQuery
    DatabaseName = 'DB'
    DataSource = DS1
    SQL.Strings = (
      'select * from CCLDS '
      'where (CCLDS.CCLDNO=:CCLDNO) and (CCLDS.DDBH=:DDBH)')
    UpdateObject = UpDet
    Left = 640
    Top = 266
    ParamData = <
      item
        DataType = ftFixedChar
        Name = 'CCLDNO'
        ParamType = ptUnknown
        Size = 11
      end
      item
        DataType = ftFixedChar
        Name = 'DDBH'
        ParamType = ptUnknown
        Size = 16
      end>
  end
