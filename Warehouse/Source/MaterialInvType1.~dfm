  object DS1: TDataSource
    DataSet = Query1
    Left = 360
    Top = 160
  end
  object Query1: TQuery
    OnNewRecord = Query1NewRecord
    DatabaseName = 'DB'
    SQL.Strings = (
      
        'select CLZl.CLDH as CLBH,CLZL.YWPM,CLZL.ZWPM,CLZL.DWBH,CLLBFLS.L' +
        'BBH,CLLBFLS.USERID'
      'from  CLZl '
      
        'left join CLLBFLS on CLLBFLS.CLBH=CLZl.CLDH and CLLBFLS.GSBH='#39'VA' +
        '3'#39
      'where CLZl.CLDH like '#39'%'#39
      '      and CLZL.YWPM like '#39'%%'#39
      '      and CLZL.YWPM like '#39'%%'#39
      '      and CLLBFLS.LBBH is null '
      ' and (exists(select KCCLMONTH.CLBH from KCCLMONTH '
      
        '            left join KCCK on KCCK.CKBH=KCCLMONTH.CKBH and KCCK.' +
        'GSBH='#39'VA3'#39
      '            where KCCLMONTH.KCYEAR='#39'2009'#39
      '                  and KCCLMONTH.KCMONTH='#39'07'#39
      '                  and KCCLMONTH.CLBH=CLZL.CLDH )'
      '   or exists(select KCLLS.CLBH from KCLLS '
      '             left join KCLL on KCLL.LLNO=KCLLS.LLNO '
      
        '             left join KCCK on KCCK.CKBH=KCLL.CKBH and KCCK.GSBH' +
        '='#39'VA3'#39
      '             where KCLL.CFMDate>getdate()-30'
      '                   and KCLLS.Qty<>0'
      '                   and KCLLS.CLBH=CLZL.CLDH )'
      '   or exists(select KCRKS.CLBH from KCRKS '
      '             left join KCRK on KCRK.RKNO=KCRKS.RKNO '
      
        '             left join KCCK on KCCK.CKBH=KCRK.CKBH and KCCK.GSBH' +
        '='#39'VA3'#39
      '             where KCRK.USERDate>getdate()-30'
      '                   and KCRKS.Qty<>0'
      '                   and KCRKS.CLBH=CLZL.CLDH )  )'
      'order by CLZl.CLDH')
    UpdateObject = UpSQL1
    Left = 328
    Top = 160
    object Query1CLBH: TStringField
      FieldName = 'CLBH'
      FixedChar = True
      Size = 10
    end
    object Query1YWPM: TStringField
      FieldName = 'YWPM'
      FixedChar = True
      Size = 200
    end
    object Query1ZWPM: TStringField
      FieldName = 'ZWPM'
      FixedChar = True
      Size = 200
    end
    object Query1DWBH: TStringField
      FieldName = 'DWBH'
      FixedChar = True
      Size = 4
    end
    object Query1LBBH: TStringField
      FieldName = 'LBBH'
      FixedChar = True
      Size = 4
    end
    object Query1USERID: TStringField
      FieldName = 'USERID'
      FixedChar = True
      Size = 15
    end
  end
  object UpSQL1: TUpdateSQL
    ModifySQL.Strings = (
      'update CLLBFYS'
      'set'
      '  GSBH = :GSBH,'
      '  CLBH = :CLBH,'
      '  FYLB = :FYLB,'
      '  USERID = :USERID,'
      '  USERDATE = :USERDATE,'
      '  YN = :YN'
      'where'
      '  GSBH = :OLD_GSBH and'
      '  CLBH = :OLD_CLBH')
    InsertSQL.Strings = (
      'insert into CLLBFYS'
      '  (GSBH, CLBH, FYLB, USERID, USERDATE, YN)'
      'values'
      '  (:GSBH, :CLBH, :FYLB, :USERID, :USERDATE, :YN)')
    DeleteSQL.Strings = (
      'delete from CLLBFYS'
      'where'
      '  GSBH = :OLD_GSBH and'
      '  CLBH = :OLD_CLBH')
    Left = 304
    Top = 160
  end
