  object Query1: TQuery
    DatabaseName = 'DB'
    SQL.Strings = (
      
        'select KCRKS.RKNO,KCRKS.CLBH,KCRKS.CGBH,KCRKS.Qty,KCRK.ZSNO,KCRK' +
        'S.FKZT,KCRK.GSBH,KCRK.CKBH,KCRK.HGLB,KCRK.USERID,KCRK.USERDATE,K' +
        'CRK.CFMID,KCRK.CFMDATE,KCRK.DOCNO,KCRKS.CNO'
      
        '       ,ZSZL.ZSDH,ZSZL.ZSYWJC,CLZL.YWPM,CLZL.ZWPM,CLZL.DWBH,KCPK' +
        '.Declaretion,KCPK.Export,KCRK.MEMO,KCRKS.RKSB,KCRKS.USPrice,KCRK' +
        'S.USACC,KCRKS.VNPrice,KCRKS.VNACC,KCRKS.RKmemo'
      'from KCRKS'
      'left join KCRK on KCRK.RKNO=KCRKS.RKNO '
      'left join ZSZL on ZSZL.ZSDH=KCRK.ZSBH '
      'left join CLZl on CLZL.CLDH=KCRKS.CLBH '
      'left join KCPK on KCRK.RKNO=KCPK.PKNO'
      
        'where convert(smalldatetime,convert(varchar,KCRK.USERDate,111)) ' +
        'between '
      '      '#39'2018/07/23'#39' and '#39'2018/07/30'#39
      '      and ZSZL.ZSYWJC like '#39'%%'#39
      '      and KCRKS.CLBH like '#39'%%'#39
      '      and KCRK.CKBH='#39'VA12'#39
      'order by KCRKS.RKNO,KCRKS.CLBH,KCRKS.RKSB')
    Left = 224
    Top = 88
