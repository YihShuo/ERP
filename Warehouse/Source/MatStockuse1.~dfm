  object Query1: TQuery
    DatabaseName = 'DB'
    SQL.Strings = (
      'select CLZl.CLDH as CLBH,CLZL.YWPM,CLZL.ZWPM,CLZL.DWBH,'
      
        '       KCCLMONTH1.Qty as OldQty,KCCLMONTH1.VNACC as OldACC,KCCLM' +
        'ONTH1.LBBH as OldBH,'
      
        '       KCCLMONTH2.Qty as newQty,KCCLMONTH2.VNACC as NewACC,KCCLM' +
        'ONTH2.LBBH as NewBH'
      'from CLZL '
      
        'left join (select KCCLMONTH.CLBH,KCCLMONTH.Qty,KCCLMONTH.VNACC,K' +
        'CCLMONTH.LBBH '
      '           from KCCLMONTH'
      
        '           left join KCCK on KCCK.CKBH=KCCLMONTH.CKBH and KCCK.G' +
        'SBH='#39'VA12'#39
      '           where KCCLMONTH.KCYEAR='#39'2009'#39
      '                 and KCCLMONTH.KCMONTH='#39'07'#39
      '           ) KCCLMONTH1 on KCCLMONTH1.CLBH=CLZL.CLDH '
      
        'left join (select KCCLMONTH.CLBH,KCCLMONTH.Qty,KCCLMONTH.VNACC,K' +
        'CCLMONTH.LBBH'
      '           from KCCLMONTH'
      
        '           left join KCCK on KCCK.CKBH=KCCLMONTH.CKBH and KCCK.G' +
        'SBH='#39'VA12'#39
      '           where KCCLMONTH.KCYEAR='#39'2009'#39
      '                 and KCCLMONTH.KCMONTH='#39'08'#39
      '           ) KCCLMONTH2 on KCCLMONTH2.CLBH=CLZL.CLDH '
      'where not (KCCLMONTH1.Qty is null and KCCLMONTH2.Qty is null) '
      '      and not (KCCLMONTH1.LBBH='#39'A'#39
      '                and KCCLMONTH2.LBBH='#39'A'#39')'
      'order by CLZL.CLDH ')
    Left = 280
    Top = 168
