  object SCSMCK: TQuery
    DatabaseName = 'DB'
    SQL.Strings = (
      
        'select  SCSMRK.DDBH,count(SCSMRK.CTNO) as CTLack,Sum(SCSMRK.Qty)' +
        ' as QtyLack,XXZL.XieXing,XXZL.SheHao,'
      
        'XXZl.XieMing,XXZl.Article,DDZL.ShipDate,SCZLDate.CTQty as CTDD,D' +
        'DZL.Pairs as QtyDD,SCSMCK.QtyCK,SCSMCK.CTCK '
      'from SCSMRK '
      'left join DDZL on SCSMRK.DDBH=DDZL.DDBH '
      
        'left join XXZL on XXZL.XieXing=DDZL.XieXIng and DDZl.SheHao=XXZl' +
        '.SheHao '
      'left join SCZLDate  on SCZLDate.ZLBH=DDZL.ZLBH '
      
        'left join (select SCSMCK.DDBH,count(SCSMCK.CTNO) as CTCK,Sum(SCS' +
        'MRK.Qty) as QtyCK'
      '            from SCSMCK '
      
        '            left join SCSMRK on SCSMRK.DDBH=SCSMCK.DDBH and SCSM' +
        'RK.CTNO=SCSMCK.CTNO '
      '            where SCSMCK.DDBH like '#39'%'#39
      
        '            group by SCSMCK.DDBH ) SCSMCK on SCSMCK.DDBH=SCSMRK.' +
        'DDBH '
      'where SCSMRK.YN='#39'2'#39
      'and XXZl.Article like '#39'%'#39
      'and XXZL.XieMing like '#39'%%'#39
      'group by  SCSMRK.DDBH,XXZL.XieXing,XXZL.SheHao,'
      
        'XXZl.XieMing,XXZl.Article,DDZL.ShipDate,SCZLDate.CTQty,DDZL.Pair' +
        's ,SCSMCK.QtyCK,SCSMCK.CTCK '
      'order by SCSMRK.DDBH ')
    Left = 184
    Top = 144
    object SCSMCKDDBH: TStringField
