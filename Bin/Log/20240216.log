select CGBJ.* ,ZSZL.ZSYWJC,ZSZl.PayVN,IsNull(ZSZLTW.zsywjc,ZSZL.zsywjc) as zsywjctw   
from CGBJ
left join ZSZL on CGBJ.ZSBH=ZSZL.ZSDH
Left join ZSZL_DEV on ZSZL_DEV.zsdh=ZSZL.zsdh and ZSZL_DEV.GSBH='VDH' 
Left join ZSZL ZSZLTW on ZSZLTW.zsdh=ZSZL_DEV.Zsdh_TW  
where CGBJ.BJNO like 'A010003774%' 
  and CGBJ.ZSBH like '%' 
  and ZSZL.ZSYWJC like '%%' 
and convert(smalldatetime,convert(varchar,CGBJ.USERDATE,111)) between 
'2024/02/13' and  '2024/02/16'
and CGBJ.GSBH='VDH'
order by CGBJ.BJNO DESC
